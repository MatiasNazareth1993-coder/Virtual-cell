        "# %%\n",
        "# @title Live Plot Demo: Shielded Adaptive RL with Dynamic Metrics\n",
        "import matplotlib.pyplot as plt\n",
        "from IPython.display import clear_output\n",
        "\n",
        "# Configure tissues (small for demo speed)\n",
        "tissues_conf = [\n",
        "    {'name': 'Liver', 'n_cells': 10, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "    {'name': 'Heart', 'n_cells': 8, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "    {'name': 'Brain', 'n_cells': 6, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "]\n",
        "\n",
        "# Initialize adaptive env + shield\n",
        "base_env = AdaptiveSafeGlobalResourceEnv(tissues_conf, total_nutrient_budget=3.0)\n",
        "shield = SafetyShield(base_env, safe_budget_fraction=0.3)\n",
        "env = ShieldedEnvWrapper(base_env, shield)\n",
        "\n",
        "# Short PPO model for demonstration\n",
        "model = PPO('MlpPolicy', env, verbose=0)\n",
        "\n",
        "# Metrics to track\n",
        "steps = []\n",
        "lambda_history = []\n",
        "damage_limit_history = []\n",
        "shield_count_history = []\n",
        "\n",
        "obs = env.reset()\n",
        "done = False\n",
        "step = 0\n",
        "shield_interventions = 0\n",
        "\n",
        "while not done and step < 100:  # short demo rollout\n",
        "    action, _ = model.predict(obs, deterministic=True)\n",
        "    obs, reward, done, info = env.step(action)\n",
        "\n",
        "    steps.append(step)\n",
        "    lambda_history.append(getattr(base_env, 'lambda_damage', 1.0))\n",
        "    damage_limit_history.append(getattr(base_env, 'dynamic_damage_limit', 200.0))\n",
        "    if info.get('shield_intervened', False):\n",
        "        shield_interventions += 1\n",
        "    shield_count_history.append(shield_interventions)\n",
        "\n",
        "    # Live plot\n",
        "    clear_output(wait=True)\n",
        "    plt.figure(figsize=(10,4))\n",
        "    plt.plot(steps, lambda_history, label='Lambda Damage')\n",
        "    plt.plot(steps, damage_limit_history, label='Dynamic Damage Limit')\n",
        "    plt.plot(steps, shield_count_history, label='Cumulative Shield Interventions')\n",
        "    plt.xlabel('Step')\n",
        "    plt.ylabel('Value')\n",
        "    plt.title('Adaptive RL Metrics with Hard Safety Shield')\n",
        "    plt.legend()\n",
        "    plt.grid(True)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "    step += 1\n",
        "\n",
        "print(\"Live plot demo complete.\")\n",
        "print(f\"Total shield interventions: {shield_interventions}\")"
      ],
      "metadata": {
        "id": "Fa2uCH40-0f9",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 442
        },
