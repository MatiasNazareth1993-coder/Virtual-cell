        "# @title 2) Visualization Utility\n",
        "\n",
        "def live_plot(t, series, labels=None, title=None):\n",
        "    plt.figure(figsize=(8,3))\n",
        "    for i, s in enumerate(series):\n",
        "        plt.plot(t, s, label=(labels[i] if labels else None))\n",
        "    if labels:\n",
        "        plt.legend()\n",
        "    if title:\n",
        "        plt.title(title)\n",
        "    plt.xlabel('Step')\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# %%\n",
        "# @title 3) Core Biological Components\n",
        "\n",
        "class SyntheticTelomere:\n",
        "    def __init__(self, initial_length=1000.0, sequence_score=0.5, degradation_rate=1.0):\n",
        "        self.L = float(initial_length)\n",
        "        self.sequence_score = float(sequence_score)\n",
        "        self.degradation_rate = float(degradation_rate)\n",
        "\n",
        "    def shorten(self, divisions=1):\n",
        "        shortening = self.degradation_rate * (1 - 0.5 * self.sequence_score)\n",
        "        self.L = max(self.L - shortening * divisions, 0.0)\n",
        "        return self.L\n",
        "\n",
        "    def lengthen_by_telomerase(self, telomerase_activity):\n",
        "        extension = 5.0 * telomerase_activity * (0.5 + 0.5 * self.sequence_score)\n",
        "        self.L += extension\n",
        "        return self.L\n",
        "\n",
        "\n",
        "class PhagocytosisSystem:\n",
        "    def __init__(self, efficiency=0.5):\n",
        "        self.efficiency = efficiency\n",
        "\n",
        "    def cleanup(self, damage_load):\n",
        "        cleanup = self.efficiency * (damage_load ** 0.8)\n",
        "        new_damage = max(damage_load - cleanup, 0.0)\n",
        "        energy_cost = 0.01 * cleanup\n",
        "        return new_damage, energy_cost\n",
        "\n",
        "\n",
        "class VirtualCell:\n",
        "    def __init__(self, telomere):\n",
        "        self.telomere = telomere\n",
        "        self.alive = True\n",
        "        self.divisions = 0\n",
        "\n",
        "    def step_divide(self, telomerase_activity, damage, nutrient):\n",
        "        if not self.alive:\n",
        "            return False\n",
        "        p_div = 1.0 / (1.0 + math.exp(-(self.telomere.L - 200.0)/100.0))\n",
        "        p_div *= nutrient * max(0.0, 1.0 - min(1.0, damage/200.0))\n",
        "        if random.random() < p_div:\n",
        "            self.telomere.shorten(1)\n",
        "            self.telomere.lengthen_by_telomerase(telomerase_activity)\n",
        "            self.divisions += 1\n",
        "            if self.telomere.L <= 50.0 or damage > 150.0:\n",
        "                self.alive = False\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "# %%\n",
        "# @title 4) Tissue and Cross-Tissue Environment with Signaling & Diffusion\n",
        "\n",
        "class Tissue:\n",
        "    def __init__(self, name, n_cells, nutrient_level, stress_factor, telomere_params=None):\n",
        "        self.name = name\n",
        "        self.nutrient_level = nutrient_level\n",
        "        self.stress_factor = stress_factor\n",
        "        self.cells = [VirtualCell(SyntheticTelomere(**(telomere_params or {}))) for _ in range(n_cells)]\n",
        "        self.damage = 0.0\n",
        "        self.phago = PhagocytosisSystem(0.6)\n",
        "        self.signaling_output = 0.0\n",
        "\n",
        "    def step(self, telomerase_activity, phago_boost):\n",
        "        base_damage = 0.5 * self.stress_factor + 0.05 * sum(1 for c in self.cells if c.alive and c.divisions > 0)\n",
        "        stress = np.random.gamma(shape=1.2, scale=self.stress_factor)\n",
        "        self.damage += base_damage + 0.2 * stress\n",
        "\n",
        "        eff = np.clip(self.phago.efficiency + phago_boost, 0, 1)\n",
        "        self.phago.efficiency = eff\n",
        "        self.damage, _ = self.phago.cleanup(self.damage)\n",
        "\n",
        "        divs = 0\n",
        "        for c in self.cells:\n",
        "            if c.alive:\n",
        "                if c.step_divide(telomerase_activity, self.damage, self.nutrient_level):\n",
        "                    divs += 1\n",
        "                    self.damage += 0.2\n",
        "\n",
        "        avg_tel = np.mean([c.telomere.L for c in self.cells if c.alive] or [0.0])\n",
        "        alive_frac = sum(1 for c in self.cells if c.alive)/len(self.cells)\n",
        "        self.signaling_output = np.tanh(self.damage/100.0) * 0.5 + np.tanh(1 - self.nutrient_level) * 0.5\n",
        "        return avg_tel, self.damage, alive_frac, divs, self.signaling_output\n",
        "\n",
        "\n",
        "class CrossTissueEnv:\n",
        "    def __init__(self, tissues_conf, diffusion_rate=0.05, signal_coupling=0.2):\n",
        "        self.tissues = [Tissue(**conf) for conf in tissues_conf]\n",
        "        self.diffusion_rate = diffusion_rate\n",
        "        self.signal_coupling = signal_coupling\n",
        "        self.time = 0\n",
        "\n",
        "    def diffuse_nutrients(self):\n",
        "        levels = np.array([t.nutrient_level for t in self.tissues])\n",
        "        mean_lvl = np.mean(levels)\n",
        "        for i, t in enumerate(self.tissues):\n",
        "            t.nutrient_level += self.diffusion_rate * (mean_lvl - t.nutrient_level)\n",
        "\n",
        "    def propagate_signals(self):\n",
        "        avg_signal = np.mean([t.signaling_output for t in self.tissues])\n",
        "        for t in self.tissues:\n",
        "            t.stress_factor *= (1 - self.signal_coupling * avg_signal)\n",
        "            t.stress_factor = max(0.1, t.stress_factor)\n",
        "\n",
        "    def step(self, actions):\n",
        "        results = []\n",
        "        for tissue, act in zip(self.tissues, actions):\n",
        "            results.append(tissue.step(*act))\n",
        "        self.diffuse_nutrients()\n",
        "        self.propagate_signals()\n",
        "        self.time += 1\n",
        "        return results\n",
        "\n",
        "# %%\n",
        "# @title 5) Safe RL Environment for Global Resource Management\n",
        "import gym\n",
        "from gym import spaces\n",
        "from stable_baselines3 import PPO\n",
        "from stable_baselines3.common.env_checker import check_env\n",
        "\n",
        "class SafeGlobalResourceEnv(gym.Env):\n",
        "    def __init__(self, tissues_conf, total_nutrient_budget=3.0):\n",
        "        super().__init__()\n",
        "        self.N = len(tissues_conf)\n",
        "        self.env = CrossTissueEnv(tissues_conf)\n",
        "        self.total_budget = float(total_nutrient_budget)\n",
        "        self.max_steps = 400\n",
        "        self.current_step = 0\n",
        "\n",
        "        self.action_space = spaces.Box(low=-2, high=2, shape=(3*self.N,), dtype=np.float32)\n",
        "        self.observation_space = spaces.Box(low=0, high=2000, shape=(5*self.N,), dtype=np.float32)\n",
        "\n",
        "        # Safety parameters\n",
        "        self.nutrient_limit = 5.0\n",
        "        self.damage_limit = 200.0\n",
        "        self.telomere_limit = (100.0, 2000.0)\n",
        "\n",
        "    def reset(self):\n",
        "        self.env = CrossTissueEnv([t for t in tissues_conf])\n",
        "        self.current_step = 0\n",
        "        return self._obs()\n",
        "\n",
        "    def _obs(self):\n",
        "        obs = []\n",
        "        for t in self.env.tissues:\n",
        "            avg_tel = np.mean([c.telomere.L for c in t.cells if c.alive] or [0.0])\n",
        "            obs.extend([avg_tel, t.damage, sum(1 for c in t.cells if c.alive)/len(t.cells), t.nutrient_level, t.signaling_output])\n",
        "        return np.array(obs, dtype=np.float32)\n",
        "\n",
        "    def _apply_safety_constraints(self, allocations, telomerase, phago):\n",
        "        # Normalize allocations with safety softmax and clamp nutrient usage\n",
        "        exp = np.exp(allocations - np.max(allocations))\n",
        "        fracs = exp / (np.sum(exp) + 1e-8)\n",
        "        safe_alloc = np.clip(fracs * self.total_budget, 0, self.nutrient_limit)\n",
        "\n",
        "        # Clamp biological controls\n",
        "        telomerase = np.clip(telomerase, 0, 1)\n",
        "        phago = np.clip(phago, -0.2, 0.5)\n",
        "\n",
        "        return safe_alloc, telomerase, phago\n",
        "\n",
        "    def step(self, action):\n",
        "        action = np.array(action, dtype=np.float32)\n",
        "        alloc, tel, ph = np.split(action, 3)\n",
        "        safe_alloc, safe_tel, safe_ph = self._apply_safety_constraints(alloc, tel, ph)\n",
        "\n",
        "        actions = []\n",
        "        for i, t in enumerate(self.env.tissues):\n",
        "            t.nutrient_level = safe_alloc[i]\n",
        "            actions.append((safe_tel[i], safe_ph[i]))\n",
        "\n",
        "        results = self.env.step(actions)\n",
        "\n",
        "        obs = self._obs()\n",
        "        reward, safety_penalty = 0.0, 0.0\n",
        "        for avg_tel, dmg, alive, divs, _ in results:\n",
        "            tel_reward = np.tanh((avg_tel - 200)/800)\n",
        "            alive_bonus = alive\n",
        "            dmg_penalty = -0.01 * dmg\n",
        "            reward += tel_reward + alive_bonus + dmg_penalty\n",
        "            # Safety penalties\n",
        "            if dmg > self.damage_limit:\n",
        "                safety_penalty -= 2.0 * (dmg/self.damage_limit - 1)\n",
        "            if avg_tel < self.telomere_limit[0] or avg_tel > self.telomere_limit[1]:\n",
        "                safety_penalty -= 1.0\n",
        "        reward += safety_penalty\n",
        "\n",
        "        done = self.current_step >= self.max_steps or all(sum(1 for c in t.cells if c.alive)==0 for t in self.env.tissues)\n",
        "        self.current_step += 1\n",
        "\n",
        "        return obs, float(reward), done, {}\n",
        "\n",
        "# %%\n",
        "# @title 6) Demo RL Training Loop with Safety Layer\n",
        "\n",
        "def train_safe_rl(total_timesteps=10000):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = SafeGlobalResourceEnv(tissues_conf)\n",
        "    check_env(env, warn=True)\n",
        "    model = PPO('MlpPolicy', env, verbose=1)\n",
        "    model.learn(total_timesteps=total_timesteps)\n",
        "    return model\n",
        "\n",
        "# %%\n",
        "# @title 7) Notes\n",
        "# - Safety constraints limit nutrient use and biological control extremes.\n",
        "# - Penalty terms discourage over-damaging or unstable telomere regimes.\n",
        "# - Safe RL supports stable long-term dynamics and avoids 'tumor-like' behavior.\n",
        "# - Extendable to include metabolic or oxygen safety regulators.\n",
        "\n",
        "# %%\n",
        "# @title 8) Adaptive Constraint Tuning\n",
        "# This section implements adaptive safety constraints that adjust over time based on\n",
        "# observed violations and environment statistics. Two complementary mechanisms are provided:\n",
        "# 1) Moving-average based safety margins (conservative -> relaxed as confidence grows).\n",
        "# 2) Lagrangian-style penalty multiplier that increases when constraints are violated and\n",
        "#    decays when satisfied.\n",
        "\n",
        "class AdaptiveSafeGlobalResourceEnv(SafeGlobalResourceEnv):\n",
        "    def __init__(self, tissues_conf, total_nutrient_budget=3.0, lr_multiplier=0.01, ma_window=50):\n",
        "        super().__init__(tissues_conf, total_nutrient_budget)\n",
        "        # Lagrangian multiplier for constraint violations\n",
        "        self.lambda_damage = 1.0\n",
        "        self.lr_multiplier = lr_multiplier\n",
        "        # moving averages for observed damage and nutrient usage\n",
        "        self.ma_window = ma_window\n",
        "        self.damage_history = deque(maxlen=ma_window)\n",
        "        self.nutr_history = deque(maxlen=ma_window)\n",
        "        # dynamic safety margins (start conservative)\n",
        "        self.dynamic_damage_limit = self.damage_limit\n",
        "        self.dynamic_nutrient_limit = self.nutrient_limit\n",
        "        self.dynamic_telomere_limit = list(self.telomere_limit)\n",
        "\n",
        "    def _update_moving_averages(self, results):\n",
        "        # results: list of (avg_tel, dmg, alive, divs, sig)\n",
        "        avg_dmg = np.mean([r[1] for r in results])\n",
        "        avg_nutr = np.mean([t.nutrient_level for t in self.env.tissues])\n",
        "        self.damage_history.append(avg_dmg)\n",
        "        self.nutr_history.append(avg_nutr)\n",
        "\n",
        "    def _adjust_dynamic_limits(self):\n",
        "        if len(self.damage_history) < 5:\n",
        "            return\n",
        "        dmg_mean = np.mean(self.damage_history)\n",
        "        nutr_mean = np.mean(self.nutr_history)\n",
        "        # If average damage is well below limit, relax limit slowly\n",
        "        if dmg_mean < 0.6 * self.damage_limit:\n",
        "            self.dynamic_damage_limit = min(self.damage_limit * 1.5, self.dynamic_damage_limit * 1.01)\n",
        "        else:\n",
        "            # tighten if damage near limit\n",
        "            self.dynamic_damage_limit = max(self.damage_limit * 0.5, self.dynamic_damage_limit * 0.995)\n",
        "        # Adjust nutrient limit if agent uses far less than cap\n",
        "        if nutr_mean < 0.5 * self.nutrient_limit:\n",
        "            self.dynamic_nutrient_limit = max(1.0, self.dynamic_nutrient_limit * 0.99)\n",
        "        else:\n",
        "            self.dynamic_nutrient_limit = min(self.nutrient_limit, self.dynamic_nutrient_limit * 1.005)\n",
        "        # Slightly adjust telomere bounds toward observed means (avoid sudden jumps)\n",
        "        # keep within safe absolute ranges\n",
        "        # (No aggressive changes here — keep human oversight in mind)\n",
        "\n",
        "    def _apply_safety_constraints(self, allocations, telomerase, phago):\n",
        "        # Use dynamic limits instead of fixed ones\n",
        "        exp = np.exp(allocations - np.max(allocations))\n",
        "        fracs = exp / (np.sum(exp) + 1e-8)\n",
        "        safe_alloc = np.clip(fracs * self.total_budget, 0, self.dynamic_nutrient_limit)\n",
        "\n",
        "        telomerase = np.clip(telomerase, 0, 1)\n",
        "        phago = np.clip(phago, -0.2, 0.5)\n",
        "        return safe_alloc, telomerase, phago\n",
        "\n",
        "    def step(self, action):\n",
        "        action = np.array(action, dtype=np.float32)\n",
        "        alloc, tel, ph = np.split(action, 3)\n",
        "        safe_alloc, safe_tel, safe_ph = self._apply_safety_constraints(alloc, tel, ph)\n",
        "\n",
        "        actions = []\n",
        "        for i, t in enumerate(self.env.tissues):\n",
        "            t.nutrient_level = float(safe_alloc[i])\n",
        "            actions.append((float(safe_tel[i]), float(safe_ph[i])))\n",
        "\n",
        "        results = self.env.step(actions)\n",
        "        # update moving averages\n",
        "        self._update_moving_averages(results)\n",
        "        # update dynamic safety margins\n",
        "        self._adjust_dynamic_limits()\n",
        "\n",
        "        # compute base reward and safety penalties (like parent) but scale penalties by lambda\n",
        "        obs = self._obs()\n",
        "        reward, safety_penalty = 0.0, 0.0\n",
        "        for avg_tel, dmg, alive, divs, _ in results:\n",
        "            tel_reward = np.tanh((avg_tel - 200)/800)\n",
        "            alive_bonus = alive\n",
        "            dmg_penalty = -0.01 * dmg\n",
        "            reward += tel_reward + alive_bonus + dmg_penalty\n",
        "            # safety penalty scaled by lambda\n",
        "            if dmg > self.dynamic_damage_limit:\n",
        "                safety_penalty -= self.lambda_damage * 2.0 * (dmg/self.dynamic_damage_limit - 1)\n",
        "            if avg_tel < self.dynamic_telomere_limit[0] or avg_tel > self.dynamic_telomere_limit[1]:\n",
        "                safety_penalty -= self.lambda_damage * 1.0\n",
        "\n",
        "        # Update lambda (dual gradient ascent): increase if violations occurred, otherwise decay\n",
        "        violation = sum(1 for r in results if r[1] > self.dynamic_damage_limit)\n",
        "        self.lambda_damage = max(0.1, self.lambda_damage + self.lr_multiplier * (violation - 0.1))\n",
        "\n",
        "        total_reward = reward + safety_penalty\n",
        "\n",
        "        self.current_step += 1\n",
        "        done = self.current_step >= self.max_steps or all(sum(1 for c in t.cells if c.alive)==0 for t in self.env.tissues)\n",
        "        return obs, float(total_reward), done, {'lambda_damage': self.lambda_damage, 'dynamic_damage_limit': self.dynamic_damage_limit}\n",
        "\n",
        "\n",
        "# Training helper that uses adaptive env\n",
        "\n",
        "def train_with_adaptive_constraints(total_timesteps=20000):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = AdaptiveSafeGlobalResourceEnv(tissues_conf, total_nutrient_budget=3.0)\n",
        "    model = PPO('MlpPolicy', env, verbose=1)\n",
        "    model.learn(total_timesteps=total_timesteps)\n",
        "    return model\n",
        "\n",
        "# %%\n",
        "# @title 9) Discussion & Best Practices\n",
        "# - Adaptive constraints allow the agent to be conservative early on and gradually relax\n",
        "#   as the environment demonstrates safe behavior.\n",
        "# - The Lagrangian multiplier (lambda) increases when violations occur, making the agent\n",
        "#   more risk-averse. Decay or floor values prevent runaway multipliers.\n",
        "# - Moving-average windows should be long enough to avoid noisy oscillations (e.g., 50-200 timesteps).\n",
        "# - Combine adaptive constraints with human-in-the-loop oversight: log dynamic limits and lambda\n",
        "#   during training and review episodes where the agent attempts to exploit relaxations.\n",
        "# - Consider adding formal safety verification layers (shielding) that override actions when\n",
        "#   critical thresholds are at imminent risk.\n",
        "\n",
        "# End of Safe RL Multi-Tissue Notebook"
      ],
      "metadata": {
        "id": "nmKp4-KH-G2N"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Virtual Cell: Multi-Tissue RL Notebook with Safety-Constrained Resource Management\n",
        "# Title: Multi-Tissue Virtual Cell with Synthetic Telomeres, Phagocytosis, Cross-Tissue Signaling, and Safe RL Resource Management\n",
        "\n",
        "# %%\n",
        "# @title 1) Setup & Dependencies\n",
        "import importlib, sys, math, random\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "try:\n",
        "    import IPython\n",
        "    IN_COLAB = 'google.colab' in str(IPython.get_ipython())\n",
        "except Exception:\n",
        "    IN_COLAB = False\n",
        "\n",
        "if any(importlib.util.find_spec(pkg) is None for pkg in (\"torch\",\"matplotlib\",\"numpy\",\"ipywidgets\")) or IN_COLAB:\n",
        "    !{sys.executable} -m pip install --quiet torch numpy matplotlib ipywidgets stable-baselines3 gym\n",
        "\n",
        "# %%\n",
        "# @title 2) Visualization Utility\n",
        "\n",
        "def live_plot(t, series, labels=None, title=None):\n",
        "    plt.figure(figsize=(8,3))\n",
        "    for i, s in enumerate(series):\n",
        "        plt.plot(t, s, label=(labels[i] if labels else None))\n",
        "    if labels:\n",
        "        plt.legend()\n",
        "    if title:\n",
        "        plt.title(title)\n",
        "    plt.xlabel('Step')\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# %%\n",
        "# @title 3) Core Biological Components\n",
        "\n",
        "class SyntheticTelomere:\n",
        "    def __init__(self, initial_length=1000.0, sequence_score=0.5, degradation_rate=1.0):\n",
        "        self.L = float(initial_length)\n",
        "        self.sequence_score = float(sequence_score)\n",
        "        self.degradation_rate = float(degradation_rate)\n",
        "\n",
        "    def shorten(self, divisions=1):\n",
        "        shortening = self.degradation_rate * (1 - 0.5 * self.sequence_score)\n",
        "        self.L = max(self.L - shortening * divisions, 0.0)\n",
        "        return self.L\n",
        "\n",
        "    def lengthen_by_telomerase(self, telomerase_activity):\n",
        "        extension = 5.0 * telomerase_activity * (0.5 + 0.5 * self.sequence_score)\n",
        "        self.L += extension\n",
        "        return self.L\n",
        "\n",
        "\n",
        "class PhagocytosisSystem:\n",
        "    def __init__(self, efficiency=0.5):\n",
        "        self.efficiency = efficiency\n",
        "\n",
        "    def cleanup(self, damage_load):\n",
        "        cleanup = self.efficiency * (damage_load ** 0.8)\n",
        "        new_damage = max(damage_load - cleanup, 0.0)\n",
        "        energy_cost = 0.01 * cleanup\n",
        "        return new_damage, energy_cost\n",
        "\n",
        "\n",
        "class VirtualCell:\n",
        "    def __init__(self, telomere):\n",
        "        self.telomere = telomere\n",
        "        self.alive = True\n",
        "        self.divisions = 0\n",
        "\n",
        "    def step_divide(self, telomerase_activity, damage, nutrient):\n",
        "        if not self.alive:\n",
        "            return False\n",
        "        p_div = 1.0 / (1.0 + math.exp(-(self.telomere.L - 200.0)/100.0))\n",
        "        p_div *= nutrient * max(0.0, 1.0 - min(1.0, damage/200.0))\n",
        "        if random.random() < p_div:\n",
        "            self.telomere.shorten(1)\n",
        "            self.telomere.lengthen_by_telomerase(telomerase_activity)\n",
        "            self.divisions += 1\n",
        "            if self.telomere.L <= 50.0 or damage > 150.0:\n",
        "                self.alive = False\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "# %%\n",
        "# @title 4) Tissue and Cross-Tissue Environment with Signaling & Diffusion\n",
        "\n",
        "class Tissue:\n",
        "    def __init__(self, name, n_cells, nutrient_level, stress_factor, telomere_params=None):\n",
        "        self.name = name\n",
        "        self.nutrient_level = nutrient_level\n",
        "        self.stress_factor = stress_factor\n",
        "        self.cells = [VirtualCell(SyntheticTelomere(**(telomere_params or {}))) for _ in range(n_cells)]\n",
        "        self.damage = 0.0\n",
        "        self.phago = PhagocytosisSystem(0.6)\n",
        "        self.signaling_output = 0.0\n",
        "\n",
        "    def step(self, telomerase_activity, phago_boost):\n",
        "        base_damage = 0.5 * self.stress_factor + 0.05 * sum(1 for c in self.cells if c.alive and c.divisions > 0)\n",
        "        stress = np.random.gamma(shape=1.2, scale=self.stress_factor)\n",
        "        self.damage += base_damage + 0.2 * stress\n",
        "\n",
        "        eff = np.clip(self.phago.efficiency + phago_boost, 0, 1)\n",
        "        self.phago.efficiency = eff\n",
        "        self.damage, _ = self.phago.cleanup(self.damage)\n",
        "\n",
        "        divs = 0\n",
        "        for c in self.cells:\n",
        "            if c.alive:\n",
        "                if c.step_divide(telomerase_activity, self.damage, self.nutrient_level):\n",
        "                    divs += 1\n",
        "                    self.damage += 0.2\n",
        "\n",
        "        avg_tel = np.mean([c.telomere.L for c in self.cells if c.alive] or [0.0])\n",
        "        alive_frac = sum(1 for c in self.cells if c.alive)/len(self.cells)\n",
        "        self.signaling_output = np.tanh(self.damage/100.0) * 0.5 + np.tanh(1 - self.nutrient_level) * 0.5\n",
        "        return avg_tel, self.damage, alive_frac, divs, self.signaling_output\n",
        "\n",
        "\n",
        "class CrossTissueEnv:\n",
        "    def __init__(self, tissues_conf, diffusion_rate=0.05, signal_coupling=0.2):\n",
        "        self.tissues = [Tissue(**conf) for conf in tissues_conf]\n",
        "        self.diffusion_rate = diffusion_rate\n",
        "        self.signal_coupling = signal_coupling\n",
        "        self.time = 0\n",
        "\n",
        "    def diffuse_nutrients(self):\n",
        "        levels = np.array([t.nutrient_level for t in self.tissues])\n",
        "        mean_lvl = np.mean(levels)\n",
        "        for i, t in enumerate(self.tissues):\n",
        "            t.nutrient_level += self.diffusion_rate * (mean_lvl - t.nutrient_level)\n",
        "\n",
        "    def propagate_signals(self):\n",
        "        avg_signal = np.mean([t.signaling_output for t in self.tissues])\n",
        "        for t in self.tissues:\n",
        "            t.stress_factor *= (1 - self.signal_coupling * avg_signal)\n",
        "            t.stress_factor = max(0.1, t.stress_factor)\n",
        "\n",
        "    def step(self, actions):\n",
        "        results = []\n",
        "        for tissue, act in zip(self.tissues, actions):\n",
        "            results.append(tissue.step(*act))\n",
        "        self.diffuse_nutrients()\n",
        "        self.propagate_signals()\n",
        "        self.time += 1\n",
        "        return results\n",
        "\n",
        "# %%\n",
        "# @title 5) Safe RL Environment for Global Resource Management\n",
        "import gym\n",
        "from gym import spaces\n",
        "from stable_baselines3 import PPO\n",
        "from stable_baselines3.common.env_checker import check_env\n",
        "\n",
        "class SafeGlobalResourceEnv(gym.Env):\n",
        "    def __init__(self, tissues_conf, total_nutrient_budget=3.0):\n",
        "        super().__init__()\n",
        "        self.N = len(tissues_conf)\n",
        "        self.env = CrossTissueEnv(tissues_conf)\n",
        "        self.total_budget = float(total_nutrient_budget)\n",
        "        self.max_steps = 400\n",
        "        self.current_step = 0\n",
        "\n",
        "        self.action_space = spaces.Box(low=-2, high=2, shape=(3*self.N,), dtype=np.float32)\n",
        "        self.observation_space = spaces.Box(low=0, high=2000, shape=(5*self.N,), dtype=np.float32)\n",
        "\n",
        "        # Safety parameters\n",
        "        self.nutrient_limit = 5.0\n",
        "        self.damage_limit = 200.0\n",
        "        self.telomere_limit = (100.0, 2000.0)\n",
        "\n",
        "    def reset(self):\n",
        "        self.env = CrossTissueEnv([t for t in tissues_conf])\n",
        "        self.current_step = 0\n",
        "        return self._obs()\n",
        "\n",
        "    def _obs(self):\n",
        "        obs = []\n",
        "        for t in self.env.tissues:\n",
        "            avg_tel = np.mean([c.telomere.L for c in t.cells if c.alive] or [0.0])\n",
        "            obs.extend([avg_tel, t.damage, sum(1 for c in t.cells if c.alive)/len(t.cells), t.nutrient_level, t.signaling_output])\n",
        "        return np.array(obs, dtype=np.float32)\n",
        "\n",
        "    def _apply_safety_constraints(self, allocations, telomerase, phago):\n",
        "        # Normalize allocations with safety softmax and clamp nutrient usage\n",
        "        exp = np.exp(allocations - np.max(allocations))\n",
        "        fracs = exp / (np.sum(exp) + 1e-8)\n",
        "        safe_alloc = np.clip(fracs * self.total_budget, 0, self.nutrient_limit)\n",
        "\n",
        "        # Clamp biological controls\n",
        "        telomerase = np.clip(telomerase, 0, 1)\n",
        "        phago = np.clip(phago, -0.2, 0.5)\n",
        "\n",
        "        return safe_alloc, telomerase, phago\n",
        "\n",
        "    def step(self, action):\n",
        "        action = np.array(action, dtype=np.float32)\n",
        "        alloc, tel, ph = np.split(action, 3)\n",
        "        safe_alloc, safe_tel, safe_ph = self._apply_safety_constraints(alloc, tel, ph)\n",
        "\n",
        "        actions = []\n",
        "        for i, t in enumerate(self.env.tissues):\n",
        "            t.nutrient_level = safe_alloc[i]\n",
        "            actions.append((safe_tel[i], safe_ph[i]))\n",
        "\n",
        "        results = self.env.step(actions)\n",
        "\n",
        "        obs = self._obs()\n",
        "        reward, safety_penalty = 0.0, 0.0\n",
        "        for avg_tel, dmg, alive, divs, _ in results:\n",
        "            tel_reward = np.tanh((avg_tel - 200)/800)\n",
        "            alive_bonus = alive\n",
        "            dmg_penalty = -0.01 * dmg\n",
        "            reward += tel_reward + alive_bonus + dmg_penalty\n",
        "            # Safety penalties\n",
        "            if dmg > self.damage_limit:\n",
        "                safety_penalty -= 2.0 * (dmg/self.damage_limit - 1)\n",
        "            if avg_tel < self.telomere_limit[0] or avg_tel > self.telomere_limit[1]:\n",
        "                safety_penalty -= 1.0\n",
        "        reward += safety_penalty\n",
        "\n",
        "        done = self.current_step >= self.max_steps or all(sum(1 for c in t.cells if c.alive)==0 for t in self.env.tissues)\n",
        "        self.current_step += 1\n",
        "\n",
        "        return obs, float(reward), done, {}\n",
        "\n",
        "# %%\n",
        "# @title 6) Demo RL Training Loop with Safety Layer\n",
        "\n",
        "def train_safe_rl(total_timesteps=10000):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = SafeGlobalResourceEnv(tissues_conf)\n",
        "    check_env(env, warn=True)\n",
        "    model = PPO('MlpPolicy', env, verbose=1)\n",
        "    model.learn(total_timesteps=total_timesteps)\n",
        "    return model\n",
        "\n",
        "# %%\n",
        "# @title 7) Notes\n",
        "# - Safety constraints limit nutrient use and biological control extremes.\n",
        "# - Penalty terms discourage over-damaging or unstable telomere regimes.\n",
        "# - Safe RL supports stable long-term dynamics and avoids 'tumor-like' behavior.\n",
        "# - Extendable to include metabolic or oxygen safety regulators.\n",
        "\n",
        "# %%\n",
        "# @title 8) Adaptive Constraint Tuning\n",
        "# This section implements adaptive safety constraints that adjust over time based on\n",
        "# observed violations and environment statistics. Two complementary mechanisms are provided:\n",
        "# 1) Moving-average based safety margins (conservative -> relaxed as confidence grows).\n",
        "# 2) Lagrangian-style penalty multiplier that increases when constraints are violated and\n",
        "#    decays when satisfied.\n",
        "\n",
        "class AdaptiveSafeGlobalResourceEnv(SafeGlobalResourceEnv):\n",
        "    def __init__(self, tissues_conf, total_nutrient_budget=3.0, lr_multiplier=0.01, ma_window=50):\n",
        "        super().__init__(tissues_conf, total_nutrient_budget)\n",
        "        # Lagrangian multiplier for constraint violations\n",
        "        self.lambda_damage = 1.0\n",
        "        self.lr_multiplier = lr_multiplier\n",
        "        # moving averages for observed damage and nutrient usage\n",
        "        self.ma_window = ma_window\n",
        "        self.damage_history = deque(maxlen=ma_window)\n",
        "        self.nutr_history = deque(maxlen=ma_window)\n",
        "        # dynamic safety margins (start conservative)\n",
        "        self.dynamic_damage_limit = self.damage_limit\n",
        "        self.dynamic_nutrient_limit = self.nutrient_limit\n",
        "        self.dynamic_telomere_limit = list(self.telomere_limit)\n",
        "\n",
        "    def _update_moving_averages(self, results):\n",
        "        # results: list of (avg_tel, dmg, alive, divs, sig)\n",
        "        avg_dmg = np.mean([r[1] for r in results])\n",
        "        avg_nutr = np.mean([t.nutrient_level for t in self.env.tissues])\n",
        "        self.damage_history.append(avg_dmg)\n",
        "        self.nutr_history.append(avg_nutr)\n",
        "\n",
        "    def _adjust_dynamic_limits(self):\n",
        "        if len(self.damage_history) < 5:\n",
        "            return\n",
        "        dmg_mean = np.mean(self.damage_history)\n",
        "        nutr_mean = np.mean(self.nutr_history)\n",
        "        # If average damage is well below limit, relax limit slowly\n",
        "        if dmg_mean < 0.6 * self.damage_limit:\n",
        "            self.dynamic_damage_limit = min(self.damage_limit * 1.5, self.dynamic_damage_limit * 1.01)\n",
        "        else:\n",
        "            # tighten if damage near limit\n",
        "            self.dynamic_damage_limit = max(self.damage_limit * 0.5, self.dynamic_damage_limit * 0.995)\n",
        "        # Adjust nutrient limit if agent uses far less than cap\n",
        "        if nutr_mean < 0.5 * self.nutrient_limit:\n",
        "            self.dynamic_nutrient_limit = max(1.0, self.dynamic_nutrient_limit * 0.99)\n",
        "        else:\n",
        "            self.dynamic_nutrient_limit = min(self.nutrient_limit, self.dynamic_nutrient_limit * 1.005)\n",
        "        # Slightly adjust telomere bounds toward observed means (avoid sudden jumps)\n",
        "        # keep within safe absolute ranges\n",
        "        # (No aggressive changes here — keep human oversight in mind)\n",
        "\n",
        "    def _apply_safety_constraints(self, allocations, telomerase, phago):\n",
        "        # Use dynamic limits instead of fixed ones\n",
        "        exp = np.exp(allocations - np.max(allocations))\n",
        "        fracs = exp / (np.sum(exp) + 1e-8)\n",
        "        safe_alloc = np.clip(fracs * self.total_budget, 0, self.dynamic_nutrient_limit)\n",
        "\n",
        "        telomerase = np.clip(telomerase, 0, 1)\n",
        "        phago = np.clip(phago, -0.2, 0.5)\n",
        "        return safe_alloc, telomerase, phago\n",
        "\n",
        "    def step(self, action):\n",
        "        action = np.array(action, dtype=np.float32)\n",
        "        alloc, tel, ph = np.split(action, 3)\n",
        "        safe_alloc, safe_tel, safe_ph = self._apply_safety_constraints(alloc, tel, ph)\n",
        "\n",
        "        actions = []\n",
        "        for i, t in enumerate(self.env.tissues):\n",
        "            t.nutrient_level = float(safe_alloc[i])\n",
        "            actions.append((float(safe_tel[i]), float(safe_ph[i])))\n",
        "\n",
        "        results = self.env.step(actions)\n",
        "        # update moving averages\n",
        "        self._update_moving_averages(results)\n",
        "        # update dynamic safety margins\n",
        "        self._adjust_dynamic_limits()\n",
        "\n",
        "        # compute base reward and safety penalties (like parent) but scale penalties by lambda\n",
        "        obs = self._obs()\n",
        "        reward, safety_penalty = 0.0, 0.0\n",
        "        for avg_tel, dmg, alive, divs, _ in results:\n",
        "            tel_reward = np.tanh((avg_tel - 200)/800)\n",
        "            alive_bonus = alive\n",
        "            dmg_penalty = -0.01 * dmg\n",
        "            reward += tel_reward + alive_bonus + dmg_penalty\n",
        "            # safety penalty scaled by lambda\n",
        "            if dmg > self.dynamic_damage_limit:\n",
        "                safety_penalty -= self.lambda_damage * 2.0 * (dmg/self.dynamic_damage_limit - 1)\n",
        "            if avg_tel < self.dynamic_telomere_limit[0] or avg_tel > self.dynamic_telomere_limit[1]:\n",
        "                safety_penalty -= self.lambda_damage * 1.0\n",
        "\n",
        "        # Update lambda (dual gradient ascent): increase if violations occurred, otherwise decay\n",
        "        violation = sum(1 for r in results if r[1] > self.dynamic_damage_limit)\n",
        "        self.lambda_damage = max(0.1, self.lambda_damage + self.lr_multiplier * (violation - 0.1))\n",
        "\n",
        "        total_reward = reward + safety_penalty\n",
        "\n",
        "        self.current_step += 1\n",
        "        done = self.current_step >= self.max_steps or all(sum(1 for c in t.cells if c.alive)==0 for t in self.env.tissues)\n",
        "        return obs, float(total_reward), done, {'lambda_damage': self.lambda_damage, 'dynamic_damage_limit': self.dynamic_damage_limit}\n",
        "\n",
        "\n",
        "# Training helper that uses adaptive env\n",
        "\n",
        "def train_with_adaptive_constraints(total_timesteps=20000):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = AdaptiveSafeGlobalResourceEnv(tissues_conf, total_nutrient_budget=3.0)\n",
        "    model = PPO('MlpPolicy', env, verbose=1)\n",
        "    model.learn(total_timesteps=total_timesteps)\n",
        "    return model\n",
        "\n",
        "# %%\n",
        "# @title 9) Discussion & Best Practices\n",
        "# - Adaptive constraints allow the agent to be conservative early on and gradually relax\n",
        "#   as the environment demonstrates safe behavior.\n",
        "# - The Lagrangian multiplier (lambda) increases when violations occur, making the agent\n",
        "#   more risk-averse. Decay or floor values prevent runaway multipliers.\n",
        "# - Moving-average windows should be long enough to avoid noisy oscillations (e.g., 50-200 timesteps).\n",
        "# - Combine adaptive constraints with human-in-the-loop oversight: log dynamic limits and lambda\n",
        "#   during training and review episodes where the agent attempts to exploit relaxations.\n",
        "# - Consider adding formal safety verification layers (shielding) that override actions when\n",
        "#   critical thresholds are at imminent risk.\n",
        "\n",
        "# %%\n",
        "# @title 10) Hard Safety Shield (Action Override)\n",
        "# This section implements a *hard safety shield* that strictly overrides any proposed\n",
        "# RL action that would lead to imminent safety violations. The shield performs a\n",
        "# one-step-ahead simulation using a deepcopy of the environment and, if a violation is\n",
        "# detected, replaces the action with a conservative safe action. All interventions are\n",
        "# logged for auditing.\n",
        "\n",
        "import copy\n",
        "from collections import deque\n",
        "\n",
        "class SafetyShield:\n",
        "    def __init__(self, env, safe_budget_fraction=0.3):\n",
        "        \"\"\"env: an instance of AdaptiveSafeGlobalResourceEnv or SafeGlobalResourceEnv\n",
        "        safe_budget_fraction: fraction of total budget to allow when overriding\"\"\"\n",
        "        self.env = env\n",
        "        self.interventions = []\n",
        "        self.safe_budget_fraction = safe_budget_fraction\n",
        "\n",
        "    def _one_step_predict(self, action):\n",
        "        # Use deepcopy to simulate one step and inspect results\n",
        "        env_copy = copy.deepcopy(self.env)\n",
        "        try:\n",
        "            obs, reward, done, info = env_copy.step(action)\n",
        "            return obs, reward, done, info\n",
        "        except Exception as e:\n",
        "            # If prediction fails, treat as potential violation\n",
        "            return None, None, None, {'error': str(e)}\n",
        "\n",
        "    def _detect_violation(self, predicted_results):\n",
        "        # predicted_results: results list from CrossTissueEnv.step() or None if failure\n",
        "        if predicted_results is None:\n",
        "            return True, 'prediction_failed'\n",
        "        # predicted_results is list of per-tissue tuples (avg_tel, dmg, alive, divs, sig)\n",
        "        for (avg_tel, dmg, alive, divs, sig) in predicted_results:\n",
        "            dmg_limit = getattr(self.env, 'dynamic_damage_limit', getattr(self.env, 'damage_limit', 200.0))\n",
        "            tel_min, tel_max = getattr(self.env, 'dynamic_telomere_limit', getattr(self.env, 'telomere_limit', (100.0, 2000.0)))\n",
        "            if dmg > dmg_limit or avg_tel < tel_min or avg_tel > tel_max:\n",
        "                reason = f'damage {dmg:.2f} vs limit {dmg_limit:.2f}' if dmg > dmg_limit else f'telomere {avg_tel:.2f} out of [{tel_min},{tel_max}]'\n",
        "                return True, reason\n",
        "        return False, None\n",
        "\n",
        "    def _conservative_action(self, action):\n",
        "        # Build a conservative action with small nutrient allocations and modest controls.\n",
        "        N = len(self.env.tissues)\n",
        "        # allocations: equal split of a reduced budget\n",
        "        alloc_budget = self.env.total_budget * self.safe_budget_fraction\n",
        "        alloc = np.full(N, alloc_budget / float(N), dtype=np.float32)\n",
        "        # telomerase: low-moderate activity to avoid runaway telomere extension\n",
        "        tel = np.full(N, 0.2, dtype=np.float32)\n",
        "        # phago: moderate positive boost to encourage cleanup\n",
        "        phago = np.full(N, 0.2, dtype=np.float32)\n",
        "        return np.concatenate([alloc, tel, phago]).astype(np.float32)\n",
        "\n",
        "    def shield_action(self, action, step_idx=None):\n",
        "        \"\"\"Return either the original action or a safe overridden action.\n",
        "        Also log interventions.\"\"\"\n",
        "        # First, map action layout expected by env.step (same as GlobalResourceEnv)\n",
        "        N = len(self.env.tissues)\n",
        "        # For environments that expect logits for allocations, action will be in that format.\n",
        "        # Our predictor expects the exact same action shape, so we try it as-is.\n",
        "        predicted = None\n",
        "        try:\n",
        "            # For AdaptiveSafeGlobalResourceEnv and SafeGlobalResourceEnv, step() expects action\n",
        "            # shaped as (3*N,) where first N are allocations/logits or actual allocations.\n",
        "            # For prediction we need to convert policies' raw action to the env.step input.\n",
        "            pred_env = copy.deepcopy(self.env)\n",
        "            # Attempt to step the copy with the action. If action is in logits form, use as-is.\n",
        "            results = pred_env.step(np.array(action, dtype=np.float32))\n",
        "            predicted = results\n",
        "        except Exception:\n",
        "            # Fallback to calling _one_step_predict which returns coarse info\n",
        "            predicted = None\n",
        "\n",
        "        violated, reason = self._detect_violation(predicted)\n",
        "        if violated:\n",
        "            safe_act = self._conservative_action(action)\n",
        "            self.interventions.append({'step': step_idx, 'reason': reason, 'original_action': action, 'safe_action': safe_act})\n",
        "            return safe_act, True\n",
        "        return action, False\n",
        "\n",
        "\n",
        "class ShieldedEnvWrapper(gym.Env):\n",
        "    def __init__(self, base_env, shield: SafetyShield):\n",
        "        super().__init__()\n",
        "        self.base_env = base_env\n",
        "        self.shield = shield\n",
        "        # mirror spaces\n",
        "        self.action_space = base_env.action_space\n",
        "        self.observation_space = base_env.observation_space\n",
        "\n",
        "    def reset(self):\n",
        "        return self.base_env.reset()\n",
        "\n",
        "    def step(self, action):\n",
        "        # Apply hard shield before executing action\n",
        "        shielded_action, intervened = self.shield.shield_action(action, step_idx=getattr(self.base_env, 'current_step', None))\n",
        "        obs, rew, done, info = self.base_env.step(shielded_action)\n",
        "        info['shield_intervened'] = intervened\n",
        "        if intervened:\n",
        "            info['shield_log'] = self.shield.interventions[-1]\n",
        "        return obs, rew, done, info\n",
        "\n",
        "    def render(self, mode='human'):\n",
        "        return self.base_env.render(mode)\n",
        "\n",
        "# %%\n",
        "# @title 11) Example: Training with Shielded Environment\n",
        "\n",
        "def train_with_shield(total_timesteps=20000):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    base_env = AdaptiveSafeGlobalResourceEnv(tissues_conf, total_nutrient_budget=3.0)\n",
        "    shield = SafetyShield(base_env, safe_budget_fraction=0.3)\n",
        "    shielded_env = ShieldedEnvWrapper(base_env, shield)\n",
        "\n",
        "    check_env(shielded_env, warn=True)\n",
        "    model = PPO('MlpPolicy', shielded_env, verbose=1)\n",
        "    model.learn(total_timesteps=total_timesteps)\n",
        "    return model, shield\n",
        "\n",
        "# %%\n",
        "# @title 12) Shield Metrics and Auditing Utilities\n",
        "\n",
        "def summarize_shield(shield: SafetyShield):\n",
        "    total = len(shield.interventions)\n",
        "    if total == 0:\n",
        "        print('No interventions recorded.')\n",
        "        return\n",
        "    reasons = {}\n",
        "    for it in shield.interventions:\n",
        "        r = it['reason']\n",
        "        reasons[r] = reasons.get(r, 0) + 1\n",
        "    print(f'Total interventions: {total}')\n",
        "    for r, c in reasons.items():\n",
        "        print(f'  {r}: {c}')\n",
        "    # show last few interventions\n",
        "    print('\\nLast interventions:')\n",
        "    for it in shield.interventions[-5:]:\n",
        "        print(it)\n",
        "\n",
        "# End of Safe RL Multi-Tissue Notebook"
      ],
      "metadata": {
        "id": "dulHDANp-eXb"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# %%\n",
        "# @title Demo: Short Shielded Adaptive RL Training Pass\n",
        "\n",
        "from stable_baselines3 import PPO\n",
        "\n",
        "# Configure tissues\n",
        "tissues_conf = [\n",
        "    {'name': 'Liver', 'n_cells': 10, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "    {'name': 'Heart', 'n_cells': 8, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "    {'name': 'Brain', 'n_cells': 6, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "]\n",
        "\n",
        "# Initialize adaptive env + shield\n",
        "base_env = AdaptiveSafeGlobalResourceEnv(tissues_conf, total_nutrient_budget=3.0)\n",
        "shield = SafetyShield(base_env, safe_budget_fraction=0.3)\n",
        "shielded_env = ShieldedEnvWrapper(base_env, shield)\n",
        "\n",
        "# Run a short training with PPO\n",
        "model = PPO('MlpPolicy', shielded_env, verbose=1)\n",
        "model.learn(total_timesteps=5000)  # short demo pass\n",
        "\n",
        "# After training, summarize shield interventions\n",
        "print(\"\\n=== Shield Intervention Summary ===\")\n",
        "summarize_shield(shield)\n",
        "\n",
        "# Optional: Test a rollout to see shield in action\n",
        "obs = shielded_env.reset()\n",
        "done = False\n",
        "step = 0\n",
        "while not done and step < 50:\n",
        "    action, _ = model.predict(obs, deterministic=True)\n",
        "    obs, reward, done, info = shielded_env.step(action)\n",
        "    if info.get('shield_intervened', False):\n",
        "        print(f\"Step {step}: Shield intervened - reason: {info['shield_log']['reason']}\")\n",
        "    step += 1\n",
        "\n",
        "print(\"Demo rollout complete.\")"
