      "source": [
        "# Virtual Cell: Colab-Ready Notebook (single-file Python script / notebook cells)\n",
        "# Title: Virtual Cell with Synthetic Telomeres, Phagocytosis, and AI Controller\n",
        "# Instructions: Open this file in Colab (File -> Upload notebook) or paste into a Colab text cell.\n",
        "# The file uses cell markers (\"# %%\") so it can be opened as a Jupyter/Colab-style script.\n",
        "\n",
        "# %%\n",
        "# @title 1) Setup & Dependencies\n",
        "# Run this cell first. It installs required packages (Colab-friendly).\n",
        "\n",
        "# If running locally, you may skip pip installs and ensure packages are available.\n",
        "try:\n",
        "    import IPython\n",
        "    IN_COLAB = 'google.colab' in str(IPython.get_ipython())\n",
        "except Exception:\n",
        "    IN_COLAB = False\n",
        "\n",
        "# Only install heavy packages when in Colab or if they're missing.\n",
        "# stable-baselines3 requires additional system packages; Colab usually supports it.\n",
        "\n",
        "REQUIRE_INSTALL = False\n",
        "import importlib\n",
        "for pkg in (\"torch\", \"matplotlib\", \"numpy\", \"ipywidgets\"):\n",
        "    if importlib.util.find_spec(pkg) is None:\n",
        "        REQUIRE_INSTALL = True\n",
        "\n",
        "if REQUIRE_INSTALL or IN_COLAB:\n",
        "    # Install minimal extras. Comment out lines you don't want to run.\n",
        "    import sys\n",
        "    !{sys.executable} -m pip install --quiet numpy matplotlib ipywidgets torch stable-baselines3 gym==0.21.0\n",
        "\n",
        "# Enable widgets in Colab (if needed)\n",
        "try:\n",
        "    from google.colab import output\n",
        "    output.enable_custom_widget_manager()\n",
        "except Exception:\n",
        "    pass\n",
        "\n",
        "print(\"Setup done.\\nPyTorch version:\", importlib.import_module('torch').__version__)\n",
        "\n",
        "# %%\n",
        "# @title 2) Imports and Utilities\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from collections import deque\n",
        "import math\n",
        "import random\n",
        "\n",
        "# Simple plotting helper\n",
        "def live_plot(t, series, labels=None, title=None, xlabel='Step'):\n",
        "    plt.figure(figsize=(8,3))\n",
        "    for i, s in enumerate(series):\n",
        "        plt.plot(t, s, label=(labels[i] if labels else None))\n",
        "    if labels:\n",
        "        plt.legend()\n",
        "    if title:\n",
        "        plt.title(title)\n",
        "    plt.xlabel(xlabel)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# %%\n",
        "# @title 3) Core simulation classes: Telomere, Cell, PhagocytosisSystem, Environment\n",
        "\n",
        "class SyntheticTelomere:\n",
        "    \"\"\"Simple abstracted telomere model.\n",
        "    L = telomere length (arbitrary units)\n",
        "    sequence_score = how well telomerase recognizes the sequence (0..1)\n",
        "    degradation_rate = baseline shortening per division or time-step\n",
        "    \"\"\"\n",
        "    def __init__(self, initial_length=1000.0, sequence_score=0.5, degradation_rate=1.0):\n",
        "        self.L = float(initial_length)\n",
        "        self.sequence_score = float(sequence_score)\n",
        "        self.degradation_rate = float(degradation_rate)\n",
        "\n",
        "    def shorten(self, divisions=1):\n",
        "        # Telomere shortening per division; sequence can reduce effective shortening\n",
        "        effective_shortening = self.degradation_rate * (1.0 - 0.5 * self.sequence_score)\n",
        "        self.L -= effective_shortening * divisions\n",
        "        self.L = max(self.L, 0.0)\n",
        "        return self.L\n",
        "\n",
        "    def lengthen_by_telomerase(self, telomerase_activity):\n",
        "        # telomerase_activity expected 0..1, sequence_score improves extension\n",
        "        extension = 5.0 * telomerase_activity * (0.5 + 0.5 * self.sequence_score)\n",
        "        self.L += extension\n",
        "        return self.L\n",
        "\n",
        "\n",
        "class PhagocytosisSystem:\n",
        "    \"\"\"Abstract phagocytosis system maintaining environmental health score (0..1).\"\"\"\n",
        "    def __init__(self, efficiency=0.5):\n",
        "        self.efficiency = float(efficiency)\n",
        "\n",
        "    def cleanup(self, damage_load):\n",
        "        # Cleanup reduces damage_load proportional to efficiency, with diminishing returns\n",
        "        cleanup = self.efficiency * (damage_load ** 0.8)\n",
        "        new_damage = max(damage_load - cleanup, 0.0)\n",
        "        # small energy cost scale (not used for now)\n",
        "        energy_cost = 0.01 * cleanup\n",
        "        return new_damage, energy_cost\n",
        "\n",
        "\n",
        "class VirtualCell:\n",
        "    \"\"\"A single virtual cell (population-level abstraction possible).\"\"\"\n",
        "    def __init__(self, telomere: SyntheticTelomere):\n",
        "        self.telomere = telomere\n",
        "        self.alive = True\n",
        "        self.divisions = 0\n",
        "\n",
        "    def step_divide(self, telomerase_activity, damage, death_threshold=100.0):\n",
        "        # Decide to divide based on telomere length and damage\n",
        "        if not self.alive:\n",
        "            return False\n",
        "        # Division probability as sigmoid of telomere length minus damage factor\n",
        "        p_div = 1.0 / (1.0 + math.exp(-(self.telomere.L - 200.0)/100.0))\n",
        "        p_div *= max(0.0, 1.0 - min(1.0, damage/200.0))\n",
        "        if random.random() < p_div:\n",
        "            # perform division\n",
        "            self.telomere.shorten(divisions=1)\n",
        "            self.telomere.lengthen_by_telomerase(telomerase_activity)\n",
        "            self.divisions += 1\n",
        "            # check death\n",
        "            if self.telomere.L <= 50.0 or damage > death_threshold:\n",
        "                self.alive = False\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "# %%\n",
        "# @title 4) Environment & Controller (AI stub and simple RL-ready wrapper)\n",
        "\n",
        "class Environment:\n",
        "    \"\"\"Environment hosting cells, damage accumulation, phagocytosis, and controller actions.\"\"\"\n",
        "    def __init__(self, n_cells=10, telomere_params=None, phago_eff=0.6):\n",
        "        self.n_cells = n_cells\n",
        "        if telomere_params is None:\n",
        "            telomere_params = {}\n",
        "        self.cells = [VirtualCell(SyntheticTelomere(**telomere_params)) for _ in range(n_cells)]\n",
        "        self.damage = 0.0\n",
        "        self.phagocytosis = PhagocytosisSystem(efficiency=phago_eff)\n",
        "        self.time = 0\n",
        "\n",
        "    def step(self, telomerase_activity, phago_boost=0.0):\n",
        "        \"\"\"Apply controller actions and advance environment one time-step.\n",
        "        telomerase_activity: 0..1\n",
        "        phago_boost: additional efficiency increment (can be negative)\n",
        "        Returns observation dict and reward metric.\n",
        "        \"\"\"\n",
        "        # 1. Damage accumulates (random + from divisions)\n",
        "        base_damage = 0.5 + 0.05 * sum(1 for c in self.cells if c.alive and c.divisions>0)\n",
        "        # environmental stochastic stress\n",
        "        stress = np.random.gamma(shape=1.2, scale=1.0)\n",
        "        self.damage += base_damage + 0.2 * stress\n",
        "\n",
        "        # 2. Phagocytosis cleanup\n",
        "        orig_eff = self.phagocytosis.efficiency\n",
        "        self.phagocytosis.efficiency = min(1.0, max(0.0, orig_eff + phago_boost))\n",
        "        self.damage, energy_cost = self.phagocytosis.cleanup(self.damage)\n",
        "        # restore baseline (phago_boost is ephemeral action)\n",
        "        self.phagocytosis.efficiency = orig_eff\n",
        "\n",
        "        # 3. Each cell attempts to divide\n",
        "        divisions = 0\n",
        "        for c in self.cells:\n",
        "            if c.alive:\n",
        "                if c.step_divide(telomerase_activity, self.damage):\n",
        "                    divisions += 1\n",
        "                    # each division slightly increases damage\n",
        "                    self.damage += 0.2\n",
        "\n",
        "        # 4. Compute simple reward: keep average telomere length high and damage low\n",
        "        avg_len = np.mean([c.telomere.L for c in self.cells if c.alive] or [0.0])\n",
        "        alive_frac = sum(1 for c in self.cells if c.alive) / float(self.n_cells)\n",
        "        reward = avg_len * 0.01 + alive_frac * 2.0 - 0.1 * self.damage - 0.5 * energy_cost\n",
        "\n",
        "        self.time += 1\n",
        "        obs = {\n",
        "            'time': self.time,\n",
        "            'avg_telomere': avg_len,\n",
        "            'damage': self.damage,\n",
        "            'alive_frac': alive_frac,\n",
        "            'divisions': divisions\n",
        "        }\n",
        "        return obs, reward\n",
        "\n",
        "\n",
        "# Simple AI controller: either a PID-like controller or a stub for RL agent\n",
        "class SimpleController:\n",
        "    def __init__(self, telomerase_target=800.0):\n",
        "        self.target = telomerase_target\n",
        "        self.last_error = 0.0\n",
        "\n",
        "    def act(self, obs):\n",
        "        # Proportional control on avg telomere\n",
        "        err = self.target - obs['avg_telomere']\n",
        "        kp = 0.002\n",
        "        telomerase_activity = float(np.clip(kp * err, 0.0, 1.0))\n",
        "        # Phagocytosis boost if damage high\n",
        "        phago_boost = float(np.clip(0.5 * (obs['damage'] - 10.0)/50.0, -0.2, 0.5))\n",
        "        return telomerase_activity, phago_boost\n",
        "\n",
        "# %%\n",
        "# @title 5) Run a baseline simulation (with simple controller) and visualize\n",
        "\n",
        "def run_simulation(steps=200, n_cells=20, telomere_params=None, phago_eff=0.6, controller=None):\n",
        "    env = Environment(n_cells=n_cells, telomere_params=telomere_params, phago_eff=phago_eff)\n",
        "    controller = controller or SimpleController()\n",
        "\n",
        "    times = []\n",
        "    avg_tel = []\n",
        "    damage = []\n",
        "    alive = []\n",
        "    divisions = []\n",
        "\n",
        "    for t in range(steps):\n",
        "        obs = {'avg_telomere': np.mean([c.telomere.L for c in env.cells if c.alive] or [0.0]),\n",
        "               'damage': env.damage,\n",
        "               'time': env.time}\n",
        "        tel_act, phago = controller.act(obs)\n",
        "        obs, rew = env.step(tel_act, phago)\n",
        "\n",
        "        times.append(t)\n",
        "        avg_tel.append(obs['avg_telomere'])\n",
        "        damage.append(obs['damage'])\n",
        "        alive.append(obs['alive_frac'])\n",
        "        divisions.append(obs['divisions'])\n",
        "\n",
        "    return dict(time=times, avg_tel=avg_tel, damage=damage, alive=alive, divisions=divisions)\n",
        "\n",
        "# Run baseline\n",
        "if __name__ == '__main__':\n",
        "    res = run_simulation(steps=300, n_cells=30, telomere_params={'initial_length':1000.0, 'sequence_score':0.7, 'degradation_rate':1.0}, phago_eff=0.6)\n",
        "    live_plot(res['time'], [res['avg_tel'], res['damage']], labels=['Avg Telomere Length','Damage'], title='Baseline Simulation')\n",
        "\n",
        "# %%\n",
        "# @title 6) Optional: Reinforcement Learning Controller (PPO using stable-baselines3)\n",
        "# NOTE: This cell provides a sketch. Training an RL agent in Colab may take time and GPU.\n",
        "\n",
        "try:\n",
        "    from stable_baselines3 import PPO\n",
        "    from stable_baselines3.common.env_checker import check_env\n",
        "    import gym\n",
        "    from gym import spaces\n",
        "\n",
        "    class GymVirtualCellEnv(gym.Env):\n",
        "        metadata = {'render.modes': ['human']}\n",
        "        def __init__(self, n_cells=10):\n",
        "            super().__init__()\n",
        "            self.env = Environment(n_cells=n_cells)\n",
        "            # Actions: continuous [telomerase_activity (0..1), phago_boost (-0.2..0.5)]\n",
        "            self.action_space = spaces.Box(low=np.array([0.0, -0.2]), high=np.array([1.0, 0.5]), dtype=np.float32)\n",
        "            # Observations: avg_telomere, damage, alive_frac\n",
        "            self.observation_space = spaces.Box(low=np.array([0.0, 0.0, 0.0]), high=np.array([2000.0, 1e6, 1.0]), dtype=np.float32)\n",
        "\n",
        "        def step(self, action):\n",
        "            tel_act = float(np.clip(action[0], 0.0, 1.0))\n",
        "            phago = float(np.clip(action[1], -0.2, 0.5))\n",
        "            obs, rew = self.env.step(tel_act, phago)\n",
        "            ob = np.array([obs['avg_telomere'], obs['damage'], obs['alive_frac']], dtype=np.float32)\n",
        "            done = False\n",
        "            # episode termination heuristics\n",
        "            if obs['alive_frac'] <= 0.0 or self.env.time > 500:\n",
        "                done = True\n",
        "            return ob, rew, done, {}\n",
        "\n",
        "        def reset(self):\n",
        "            self.env = Environment(n_cells=self.env.n_cells)\n",
        "            return np.array([self.env.damage, 0.0, 1.0], dtype=np.float32)\n",
        "\n",
        "        def render(self, mode='human'):\n",
        "            pass\n",
        "\n",
        "    # Example training snippet (fast demo)\n",
        "    def train_demo(steps=20000):\n",
        "        gym_env = GymVirtualCellEnv(n_cells=20)\n",
        "        check_env(gym_env, warn=True)\n",
        "        model = PPO('MlpPolicy', gym_env, verbose=1)\n",
        "        model.learn(total_timesteps=steps)\n",
        "        return model\n",
        "\n",
        "except Exception as e:\n",
        "    print('RL components unavailable or failed to import:', e)\n",
        "\n",
        "# %%\n",
        "# @title 7) Interactive demo with ipywidgets (Colab-friendly)\n",
        "\n",
        "try:\n",
        "    import ipywidgets as widgets\n",
        "    from IPython.display import display, clear_output\n",
        "\n",
        "    def interactive_demo():\n",
        "        steps = widgets.IntSlider(min=50, max=1000, step=50, value=300, description='Steps')\n",
        "        n_cells = widgets.IntSlider(min=1, max=200, step=1, value=30, description='Cells')\n",
        "        seq_score = widgets.FloatSlider(min=0.0, max=1.0, step=0.01, value=0.7, description='Sequence score')\n",
        "        phago_eff = widgets.FloatSlider(min=0.0, max=1.0, step=0.01, value=0.6, description='Phago eff')\n",
        "        run_button = widgets.Button(description='Run Simulation')\n",
        "\n",
        "        out = widgets.Output()\n",
        "\n",
        "        def on_run(b):\n",
        "            with out:\n",
        "                clear_output(wait=True)\n",
        "                tel_params = {'initial_length':1000.0, 'sequence_score':seq_score.value, 'degradation_rate':1.0}\n",
        "                res = run_simulation(steps=steps.value, n_cells=n_cells.value, telomere_params=tel_params, phago_eff=phago_eff.value)\n",
        "                live_plot(res['time'], [res['avg_tel'], res['damage']], labels=['Avg Telomere Length','Damage'], title='Simulation')\n",
        "                live_plot(res['time'], [res['alive'], res['divisions']], labels=['Alive Fraction','Divisions'], title='Population Metrics')\n",
        "\n",
        "        run_button.on_click(on_run)\n",
        "        display(widgets.VBox([widgets.HBox([steps, n_cells]), widgets.HBox([seq_score, phago_eff]), run_button, out]))\n",
        "\n",
        "except Exception as e:\n",
        "    print('Interactive widgets unavailable:', e)\n",
        "\n",
        "# %%\n",
        "# @title 8) Notes, Extensions, and Next Steps\n",
        "# - This notebook provides a high-level, runnable simulation of telomere maintenance + phagocytosis.\n",
        "# - Extend molecular fidelity by replacing the SyntheticTelomere with sequence-specific models\n",
        "#  (e.g., binding energy predictions from a pre-trained model or MD simulations).\n",
        "# - The RL environment is intentionally simple; for research-grade RL, tune reward shaping,\n",
        "#  use curriculum learning, and add safety constraints to avoid runaway proliferation.\n",
        "# - Consider adding checkpoints, video export (matplotlib animation -> mp4), and experiment logging.\n",
        "\n",
        "# End of notebook"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Virtual Cell: Multi-Tissue Colab-Ready Notebook\n",
        "# Title: Multi-Tissue Virtual Cell with Synthetic Telomeres, Phagocytosis, and AI Controller\n",
        "# This version extends the single-tissue simulation to include multiple tissue compartments,\n",
        "# each with distinct telomere dynamics, nutrient gradients, and environmental stresses.\n",
        "\n",
        "# %%\n",
        "# @title 1) Setup & Dependencies\n",
        "try:\n",
        "    import IPython\n",
        "    IN_COLAB = 'google.colab' in str(IPython.get_ipython())\n",
        "except Exception:\n",
        "    IN_COLAB = False\n",
        "\n",
        "import importlib\n",
        "import sys\n",
        "\n",
        "REQUIRED = [\"torch\", \"numpy\", \"matplotlib\", \"ipywidgets\"]\n",
        "if any(importlib.util.find_spec(pkg) is None for pkg in REQUIRED) or IN_COLAB:\n",
        "    !{sys.executable} -m pip install --quiet torch numpy matplotlib ipywidgets stable-baselines3 gym==0.21.0\n",
        "\n",
        "try:\n",
        "    from google.colab import output\n",
        "    output.enable_custom_widget_manager()\n",
        "except Exception:\n",
        "    pass\n",
        "\n",
        "# %%\n",
        "# @title 2) Imports and Utilities\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import random, math\n",
        "\n",
        "\n",
        "def live_plot(t, series, labels=None, title=None):\n",
        "    plt.figure(figsize=(8,3))\n",
        "    for i, s in enumerate(series):\n",
        "        plt.plot(t, s, label=(labels[i] if labels else None))\n",
        "    if labels:\n",
        "        plt.legend()\n",
        "    if title:\n",
        "        plt.title(title)\n",
        "    plt.xlabel('Step')\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# %%\n",
        "# @title 3) Core Simulation Classes\n",
        "\n",
        "class SyntheticTelomere:\n",
        "    def __init__(self, initial_length=1000.0, sequence_score=0.5, degradation_rate=1.0):\n",
        "        self.L = float(initial_length)\n",
        "        self.sequence_score = float(sequence_score)\n",
        "        self.degradation_rate = float(degradation_rate)\n",
        "\n",
        "    def shorten(self, divisions=1):\n",
        "        effective_shortening = self.degradation_rate * (1 - 0.5 * self.sequence_score)\n",
        "        self.L = max(self.L - effective_shortening * divisions, 0.0)\n",
        "        return self.L\n",
        "\n",
        "    def lengthen_by_telomerase(self, telomerase_activity):\n",
        "        extension = 5.0 * telomerase_activity * (0.5 + 0.5 * self.sequence_score)\n",
        "        self.L += extension\n",
        "        return self.L\n",
        "\n",
        "\n",
        "class PhagocytosisSystem:\n",
        "    def __init__(self, efficiency=0.5):\n",
        "        self.efficiency = float(efficiency)\n",
        "\n",
        "    def cleanup(self, damage_load):\n",
        "        cleanup = self.efficiency * (damage_load ** 0.8)\n",
        "        new_damage = max(damage_load - cleanup, 0.0)\n",
        "        energy_cost = 0.01 * cleanup\n",
        "        return new_damage, energy_cost\n",
        "\n",
        "\n",
        "class VirtualCell:\n",
        "    def __init__(self, telomere: SyntheticTelomere):\n",
        "        self.telomere = telomere\n",
        "        self.alive = True\n",
        "        self.divisions = 0\n",
        "\n",
        "    def step_divide(self, telomerase_activity, damage, nutrient):\n",
        "        if not self.alive:\n",
        "            return False\n",
        "        p_div = 1.0 / (1.0 + math.exp(-(self.telomere.L - 200.0)/100.0))\n",
        "        p_div *= nutrient * max(0.0, 1.0 - min(1.0, damage/200.0))\n",
        "        if random.random() < p_div:\n",
        "            self.telomere.shorten(1)\n",
        "            self.telomere.lengthen_by_telomerase(telomerase_activity)\n",
        "            self.divisions += 1\n",
        "            if self.telomere.L <= 50.0 or damage > 150.0:\n",
        "                self.alive = False\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "# %%\n",
        "# @title 4) Multi-Tissue Environment\n",
        "\n",
        "class Tissue:\n",
        "    def __init__(self, name, n_cells=20, nutrient_level=1.0, stress_factor=1.0, telomere_params=None):\n",
        "        self.name = name\n",
        "        self.nutrient_level = nutrient_level\n",
        "        self.stress_factor = stress_factor\n",
        "        self.cells = [VirtualCell(SyntheticTelomere(**(telomere_params or {}))) for _ in range(n_cells)]\n",
        "        self.damage = 0.0\n",
        "        self.phago = PhagocytosisSystem(efficiency=0.6)\n",
        "\n",
        "    def step(self, telomerase_activity, phago_boost):\n",
        "        base_damage = 0.5 * self.stress_factor + 0.05 * sum(1 for c in self.cells if c.alive and c.divisions > 0)\n",
        "        stress = np.random.gamma(shape=1.2, scale=self.stress_factor)\n",
        "        self.damage += base_damage + 0.2 * stress\n",
        "\n",
        "        eff = min(1.0, max(0.0, self.phago.efficiency + phago_boost))\n",
        "        self.phago.efficiency = eff\n",
        "        self.damage, _ = self.phago.cleanup(self.damage)\n",
        "\n",
        "        divs = 0\n",
        "        for c in self.cells:\n",
        "            if c.alive:\n",
        "                if c.step_divide(telomerase_activity, self.damage, self.nutrient_level):\n",
        "                    divs += 1\n",
        "                    self.damage += 0.2\n",
        "        avg_tel = np.mean([c.telomere.L for c in self.cells if c.alive] or [0.0])\n",
        "        alive_frac = sum(1 for c in self.cells if c.alive) / len(self.cells)\n",
        "        return avg_tel, self.damage, alive_frac, divs\n",
        "\n",
        "\n",
        "class MultiTissueEnv:\n",
        "    def __init__(self, tissues_conf):\n",
        "        self.tissues = [Tissue(**conf) for conf in tissues_conf]\n",
        "        self.time = 0\n",
        "\n",
        "    def step(self, actions):\n",
        "        results = []\n",
        "        for tissue, act in zip(self.tissues, actions):\n",
        "            results.append(tissue.step(*act))\n",
        "        self.time += 1\n",
        "        return results\n",
        "\n",
        "# %%\n",
        "# @title 5) Controllers\n",
        "\n",
        "class SimpleMultiController:\n",
        "    def __init__(self, targets):\n",
        "        self.targets = targets\n",
        "\n",
        "    def act(self, tissue_obs):\n",
        "        actions = []\n",
        "        for obs, target in zip(tissue_obs, self.targets):\n",
        "            avg_tel, damage, alive_frac, _ = obs\n",
        "            err = target - avg_tel\n",
        "            telomerase = np.clip(0.002 * err, 0, 1)\n",
        "            phago = np.clip(0.5 * (damage - 10.0)/50.0, -0.2, 0.5)\n",
        "            actions.append((telomerase, phago))\n",
        "        return actions\n",
        "\n",
        "# %%\n",
        "# @title 6) Run Multi-Tissue Simulation\n",
        "\n",
        "def run_multitissue(steps=200):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = MultiTissueEnv(tissues_conf)\n",
        "    ctrl = SimpleMultiController([800.0, 700.0, 900.0])\n",
        "\n",
        "    hist = {t['name']: {'tel': [], 'damage': [], 'alive': [], 'divs': []} for t in tissues_conf}\n",
        "    for _ in range(steps):\n",
        "        obs = []\n",
        "        for tissue in env.tissues:\n",
        "            avg_tel = np.mean([c.telomere.L for c in tissue.cells if c.alive] or [0.0])\n",
        "            obs.append((avg_tel, tissue.damage, sum(1 for c in tissue.cells if c.alive)/len(tissue.cells), 0))\n",
        "        actions = ctrl.act(obs)\n",
        "        results = env.step(actions)\n",
        "        for tissue, r in zip(env.tissues, results):\n",
        "            avg_tel, dmg, alive, divs = r\n",
        "            h = hist[tissue.name]\n",
        "            h['tel'].append(avg_tel)\n",
        "            h['damage'].append(dmg)\n",
        "            h['alive'].append(alive)\n",
        "            h['divs'].append(divs)\n",
        "\n",
        "    for name, h in hist.items():\n",
        "        live_plot(range(steps), [h['tel'], h['damage']], labels=['Telomere','Damage'], title=f'{name} Tissue Dynamics')\n",
        "        live_plot(range(steps), [h['alive'], h['divs']], labels=['Alive','Divisions'], title=f'{name} Population Metrics')\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    run_multitissue(steps=300)\n",
        "\n",
        "# %%\n",
        "# @title 7) Extensions\n",
        "# - Introduce nutrient diffusion and cross-tissue communication.\n",
        "# - Add dynamic telomerase regulation signals shared between tissues.\n",
        "# - Extend RL controller for adaptive global control.\n",
        "# - Visualize tissue-level 3D nutrient or damage gradients.\n",
        "# - Integrate real molecular models for T-loop structure adaptation.\n",
        "\n",
        "# End of multi-tissue notebook"
      ],
