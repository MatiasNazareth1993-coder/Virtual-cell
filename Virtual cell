    {
      "cell_type": "code",
      "source": [
        "# Virtual Cell: Multi-Tissue Notebook with Cross-Tissue Nutrient Diffusion & Signaling\n",
        "# Title: Multi-Tissue Virtual Cell with Synthetic Telomeres, Phagocytosis, and Inter-Tissue Communication\n",
        "\n",
        "# %%\n",
        "# @title 1) Setup & Dependencies\n",
        "import importlib, sys, math, random\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "try:\n",
        "    import IPython\n",
        "    IN_COLAB = 'google.colab' in str(IPython.get_ipython())\n",
        "except Exception:\n",
        "    IN_COLAB = False\n",
        "\n",
        "if any(importlib.util.find_spec(pkg) is None for pkg in (\"torch\",\"matplotlib\",\"numpy\",\"ipywidgets\")) or IN_COLAB:\n",
        "    !{sys.executable} -m pip install --quiet torch numpy matplotlib ipywidgets\n",
        "\n",
        "# %%\n",
        "# @title 2) Visualization Utility\n",
        "\n",
        "def live_plot(t, series, labels=None, title=None):\n",
        "    plt.figure(figsize=(8,3))\n",
        "    for i, s in enumerate(series):\n",
        "        plt.plot(t, s, label=(labels[i] if labels else None))\n",
        "    if labels:\n",
        "        plt.legend()\n",
        "    if title:\n",
        "        plt.title(title)\n",
        "    plt.xlabel('Step')\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# %%\n",
        "# @title 3) Core Biological Classes\n",
        "\n",
        "class SyntheticTelomere:\n",
        "    def __init__(self, initial_length=1000.0, sequence_score=0.5, degradation_rate=1.0):\n",
        "        self.L = float(initial_length)\n",
        "        self.sequence_score = float(sequence_score)\n",
        "        self.degradation_rate = float(degradation_rate)\n",
        "\n",
        "    def shorten(self, divisions=1):\n",
        "        shortening = self.degradation_rate * (1 - 0.5 * self.sequence_score)\n",
        "        self.L = max(self.L - shortening * divisions, 0.0)\n",
        "        return self.L\n",
        "\n",
        "    def lengthen_by_telomerase(self, telomerase_activity):\n",
        "        extension = 5.0 * telomerase_activity * (0.5 + 0.5 * self.sequence_score)\n",
        "        self.L += extension\n",
        "        return self.L\n",
        "\n",
        "\n",
        "class PhagocytosisSystem:\n",
        "    def __init__(self, efficiency=0.5):\n",
        "        self.efficiency = efficiency\n",
        "\n",
        "    def cleanup(self, damage_load):\n",
        "        cleanup = self.efficiency * (damage_load ** 0.8)\n",
        "        new_damage = max(damage_load - cleanup, 0.0)\n",
        "        energy_cost = 0.01 * cleanup\n",
        "        return new_damage, energy_cost\n",
        "\n",
        "\n",
        "class VirtualCell:\n",
        "    def __init__(self, telomere):\n",
        "        self.telomere = telomere\n",
        "        self.alive = True\n",
        "        self.divisions = 0\n",
        "\n",
        "    def step_divide(self, telomerase_activity, damage, nutrient):\n",
        "        if not self.alive:\n",
        "            return False\n",
        "        p_div = 1.0 / (1.0 + math.exp(-(self.telomere.L - 200.0)/100.0))\n",
        "        p_div *= nutrient * max(0.0, 1.0 - min(1.0, damage/200.0))\n",
        "        if random.random() < p_div:\n",
        "            self.telomere.shorten(1)\n",
        "            self.telomere.lengthen_by_telomerase(telomerase_activity)\n",
        "            self.divisions += 1\n",
        "            if self.telomere.L <= 50.0 or damage > 150.0:\n",
        "                self.alive = False\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "# %%\n",
        "# @title 4) Tissue and Cross-Tissue Environment\n",
        "\n",
        "class Tissue:\n",
        "    def __init__(self, name, n_cells, nutrient_level, stress_factor, telomere_params=None):\n",
        "        self.name = name\n",
        "        self.nutrient_level = nutrient_level\n",
        "        self.stress_factor = stress_factor\n",
        "        self.cells = [VirtualCell(SyntheticTelomere(**(telomere_params or {}))) for _ in range(n_cells)]\n",
        "        self.damage = 0.0\n",
        "        self.phago = PhagocytosisSystem(0.6)\n",
        "        self.signaling_output = 0.0\n",
        "\n",
        "    def step(self, telomerase_activity, phago_boost):\n",
        "        base_damage = 0.5 * self.stress_factor + 0.05 * sum(1 for c in self.cells if c.alive and c.divisions > 0)\n",
        "        stress = np.random.gamma(shape=1.2, scale=self.stress_factor)\n",
        "        self.damage += base_damage + 0.2 * stress\n",
        "\n",
        "        eff = np.clip(self.phago.efficiency + phago_boost, 0, 1)\n",
        "        self.phago.efficiency = eff\n",
        "        self.damage, _ = self.phago.cleanup(self.damage)\n",
        "\n",
        "        divs = 0\n",
        "        for c in self.cells:\n",
        "            if c.alive:\n",
        "                if c.step_divide(telomerase_activity, self.damage, self.nutrient_level):\n",
        "                    divs += 1\n",
        "                    self.damage += 0.2\n",
        "\n",
        "        avg_tel = np.mean([c.telomere.L for c in self.cells if c.alive] or [0.0])\n",
        "        alive_frac = sum(1 for c in self.cells if c.alive)/len(self.cells)\n",
        "        # signaling output proportional to stress & nutrient consumption\n",
        "        self.signaling_output = np.tanh(self.damage/100.0) * 0.5 + np.tanh(1 - self.nutrient_level) * 0.5\n",
        "        return avg_tel, self.damage, alive_frac, divs, self.signaling_output\n",
        "\n",
        "\n",
        "class CrossTissueEnv:\n",
        "    def __init__(self, tissues_conf, diffusion_rate=0.05, signal_coupling=0.2):\n",
        "        self.tissues = [Tissue(**conf) for conf in tissues_conf]\n",
        "        self.diffusion_rate = diffusion_rate\n",
        "        self.signal_coupling = signal_coupling\n",
        "        self.time = 0\n",
        "\n",
        "    def diffuse_nutrients(self):\n",
        "        levels = np.array([t.nutrient_level for t in self.tissues])\n",
        "        mean_lvl = np.mean(levels)\n",
        "        for i, t in enumerate(self.tissues):\n",
        "            t.nutrient_level += self.diffusion_rate * (mean_lvl - t.nutrient_level)\n",
        "\n",
        "    def propagate_signals(self):\n",
        "        avg_signal = np.mean([t.signaling_output for t in self.tissues])\n",
        "        for t in self.tissues:\n",
        "            # reduce stress if others signal high support\n",
        "            t.stress_factor *= (1 - self.signal_coupling * avg_signal)\n",
        "            t.stress_factor = max(0.1, t.stress_factor)\n",
        "\n",
        "    def step(self, actions):\n",
        "        results = []\n",
        "        for tissue, act in zip(self.tissues, actions):\n",
        "            results.append(tissue.step(*act))\n",
        "        self.diffuse_nutrients()\n",
        "        self.propagate_signals()\n",
        "        self.time += 1\n",
        "        return results\n",
        "\n",
        "# %%\n",
        "# @title 5) Controllers\n",
        "\n",
        "class SimpleMultiController:\n",
        "    def __init__(self, targets):\n",
        "        self.targets = targets\n",
        "\n",
        "    def act(self, tissue_obs):\n",
        "        actions = []\n",
        "        for obs, target in zip(tissue_obs, self.targets):\n",
        "            avg_tel, damage, alive, _, _ = obs\n",
        "            err = target - avg_tel\n",
        "            telomerase = np.clip(0.002 * err, 0, 1)\n",
        "            phago = np.clip(0.5 * (damage - 10.0)/50.0, -0.2, 0.5)\n",
        "            actions.append((telomerase, phago))\n",
        "        return actions\n",
        "\n",
        "# %%\n",
        "# @title 6) Simulation Runner\n",
        "\n",
        "def run_cross_tissue(steps=300):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = CrossTissueEnv(tissues_conf)\n",
        "    ctrl = SimpleMultiController([800.0, 700.0, 900.0])\n",
        "\n",
        "    hist = {t['name']: {'tel': [], 'damage': [], 'alive': [], 'divs': [], 'nutrient': []} for t in tissues_conf}\n",
        "    for _ in range(steps):\n",
        "        obs = []\n",
        "        for tissue in env.tissues:\n",
        "            avg_tel = np.mean([c.telomere.L for c in tissue.cells if c.alive] or [0.0])\n",
        "            obs.append((avg_tel, tissue.damage, sum(1 for c in tissue.cells if c.alive)/len(tissue.cells), 0, tissue.signaling_output))\n",
        "        actions = ctrl.act(obs)\n",
        "        results = env.step(actions)\n",
        "        for tissue, r in zip(env.tissues, results):\n",
        "            avg_tel, dmg, alive, divs, _ = r\n",
        "            h = hist[tissue.name]\n",
        "            h['tel'].append(avg_tel)\n",
        "            h['damage'].append(dmg)\n",
        "            h['alive'].append(alive)\n",
        "            h['divs'].append(divs)\n",
        "            h['nutrient'].append(tissue.nutrient_level)\n",
        "\n",
        "    for name, h in hist.items():\n",
        "        live_plot(range(steps), [h['tel'], h['damage']], labels=['Telomere','Damage'], title=f'{name} Tissue Dynamics')\n",
        "        live_plot(range(steps), [h['alive'], h['divs']], labels=['Alive','Divisions'], title=f'{name} Population Metrics')\n",
        "        live_plot(range(steps), [h['nutrient']], labels=['Nutrient'], title=f'{name} Nutrient Diffusion')\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    run_cross_tissue(steps=300)\n",
        "\n",
        "# %%\n",
        "# @title 7) Extensions\n",
        "# - Implement spatial 2D diffusion maps for signaling.\n",
        "# - Add per-tissue telomerase regulation feedback.\n",
        "# - Introduce metabolic network coupling.\n",
        "# - Apply reinforcement learning for global resource management.\n",
        "\n",
        "# End of cross-tissue notebook\n",
        "\n",
        "# %%\n",
        "# @title 8) Reinforcement Learning — Global Resource Management (PPO)\n",
        "# This section provides a Gym-compatible environment that exposes global resource allocation\n",
        "# actions: the agent allocates a finite nutrient budget across tissues each timestep and\n",
        "# sets per-tissue telomerase & phagocytosis controls. The agent learns to maximize a\n",
        "# multi-objective reward (maintain telomeres, minimize damage, keep cells alive) under\n",
        "# resource constraints.\n",
        "\n",
        "try:\n",
        "    import gym\n",
        "    from gym import spaces\n",
        "    from stable_baselines3 import PPO\n",
        "    from stable_baselines3.common.env_checker import check_env\n",
        "    import torch\n",
        "except Exception as e:\n",
        "    print('RL dependencies not available — run pip install stable-baselines3 gym torch')\n",
        "    # continue gracefully; RL cells will still be present but training will fail if imports missing\n",
        "\n",
        "\n",
        "class GlobalResourceEnv(gym.Env):\n",
        "    \"\"\"Gym environment wrapping the CrossTissueEnv for RL.\n",
        "    Action vector layout (for N tissues):\n",
        "      - first N entries: raw nutrient allocation logits (these will be softmaxed to form fractions)\n",
        "      - next N entries: telomerase activity per tissue (clipped 0..1)\n",
        "      - next N entries: phagocytosis boost per tissue (clipped -0.2..0.5)\n",
        "    Observation:\n",
        "      - concatenated per-tissue [avg_tel, damage, alive_frac, nutrient_level, signaling_output]\n",
        "    \"\"\"\n",
        "    def __init__(self, tissues_conf, total_nutrient_budget=3.0, diffusion_rate=0.05, signal_coupling=0.2):\n",
        "        super().__init__()\n",
        "        self.tissues_conf = tissues_conf\n",
        "        self.N = len(tissues_conf)\n",
        "        self.total_budget = float(total_nutrient_budget)\n",
        "        self.env = CrossTissueEnv(tissues_conf, diffusion_rate=diffusion_rate, signal_coupling=signal_coupling)\n",
        "\n",
        "        # Actions: 3*N continuous values\n",
        "        low = np.concatenate([np.full(self.N, -10.0), np.zeros(self.N), np.full(self.N, -0.2)])\n",
        "        high = np.concatenate([np.full(self.N, 10.0), np.ones(self.N), np.full(self.N, 0.5)])\n",
        "        self.action_space = spaces.Box(low=low, high=high, dtype=np.float32)\n",
        "\n",
        "        # Observations: 5*N\n",
        "        obs_low = np.zeros(5 * self.N, dtype=np.float32)\n",
        "        obs_high = np.concatenate([np.full(self.N, 2000.0), np.full(self.N, 1e6), np.ones(self.N), np.full(self.N, 5.0), np.ones(self.N)])\n",
        "        self.observation_space = spaces.Box(low=obs_low, high=obs_high, dtype=np.float32)\n",
        "\n",
        "        self.max_steps = 500\n",
        "        self.current_step = 0\n",
        "\n",
        "    def reset(self):\n",
        "        self.env = CrossTissueEnv(self.tissues_conf)\n",
        "        self.current_step = 0\n",
        "        return self._get_obs()\n",
        "\n",
        "    def _get_obs(self):\n",
        "        obs = []\n",
        "        for t in self.env.tissues:\n",
        "            avg_tel = np.mean([c.telomere.L for c in t.cells if c.alive] or [0.0])\n",
        "            obs.extend([avg_tel, t.damage, sum(1 for c in t.cells if c.alive)/len(t.cells), t.nutrient_level, t.signaling_output])\n",
        "        return np.array(obs, dtype=np.float32)\n",
        "\n",
        "    def step(self, action):\n",
        "        # parse actions\n",
        "        action = np.array(action, dtype=np.float32)\n",
        "        logits = action[:self.N]\n",
        "        tel_act = np.clip(action[self.N:2*self.N], 0.0, 1.0)\n",
        "        phago = np.clip(action[2*self.N:3*self.N], -0.2, 0.5)\n",
        "\n",
        "        # nutrient allocation via softmax over logits scaled by total budget\n",
        "        exp = np.exp(logits - np.max(logits))\n",
        "        fracs = exp / (np.sum(exp) + 1e-8)\n",
        "        allocations = fracs * self.total_budget\n",
        "\n",
        "        # apply allocations by setting tissue nutrient levels (additive, bounded)\n",
        "        actions = []\n",
        "        for i, t in enumerate(self.env.tissues):\n",
        "            # set nutrient level to allocated value (bounded 0..5)\n",
        "            t.nutrient_level = float(np.clip(allocations[i], 0.0, 5.0))\n",
        "            actions.append((float(tel_act[i]), float(phago[i])))\n",
        "\n",
        "        results = self.env.step(actions)\n",
        "\n",
        "        # form observation\n",
        "        obs = self._get_obs()\n",
        "\n",
        "        # reward: weighted combination across tissues\n",
        "        total_reward = 0.0\n",
        "        for (avg_tel, dmg, alive, divs, sig), t in zip(results, self.env.tissues):\n",
        "            # telomere benefit saturates: use tanh\n",
        "            tel_reward = np.tanh(avg_tel / 1000.0)\n",
        "            damage_penalty = -0.01 * dmg\n",
        "            alive_bonus = 1.0 * alive\n",
        "            # small penalty for high nutrient usage to encourage efficiency\n",
        "            nutrient_penalty = -0.05 * (t.nutrient_level / self.total_budget)\n",
        "            total_reward += tel_reward + damage_penalty + alive_bonus + nutrient_penalty\n",
        "\n",
        "        self.current_step += 1\n",
        "        done = False\n",
        "        if self.current_step >= self.max_steps or all(sum(1 for c in t.cells if c.alive)==0 for t in self.env.tissues):\n",
        "            done = True\n",
        "\n",
        "        info = {}\n",
        "        return obs, float(total_reward), done, info\n",
        "\n",
        "\n",
        "# Quick environment sanity check function\n",
        "def build_and_check_env():\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 20, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 18, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 15, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = GlobalResourceEnv(tissues_conf)\n",
        "    check_env(env, warn=True)\n",
        "    return env\n",
        "\n",
        "\n",
        "# Training helper (small demo training loop)\n",
        "def train_demo(total_timesteps=20000, save_path='ppo_global_resource.zip'):\n",
        "    tissues_conf = [\n",
        "        {'name': 'Liver', 'n_cells': 30, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "        {'name': 'Heart', 'n_cells': 25, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "        {'name': 'Brain', 'n_cells': 20, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "    ]\n",
        "    env = GlobalResourceEnv(tissues_conf, total_nutrient_budget=3.0)\n",
        "    model = PPO('MlpPolicy', env, verbose=1, tensorboard_log='./ppo_resource_tb/')\n",
        "    model.learn(total_timesteps=total_timesteps)\n",
        "    model.save(save_path)\n",
        "    return model\n",
        "\n",
        "\n",
        "# Evaluation snippet\n",
        "def evaluate_model(model, episodes=5):\n",
        "    env = model.get_env()\n",
        "    returns = []\n",
        "    for ep in range(episodes):\n",
        "        obs = env.reset()\n",
        "        done = False\n",
        "        total = 0.0\n",
        "        while not done:\n",
        "            action, _ = model.predict(obs, deterministic=True)\n",
        "            obs, rew, done, _ = env.step(action)\n",
        "            total += rew\n",
        "        returns.append(total)\n",
        "    print('Average return:', np.mean(returns))\n",
        "    return returns\n",
        "\n",
        "# %%\n",
        "# @title 9) Notes on RL\n",
        "# - The action design uses softmax to ensure nutrient allocations respect the global budget.\n",
        "# - Reward shaping combines telomere maintenance, damage minimization, and alive cell fraction;\n",
        "#   tune coefficients for desired trade-offs.\n",
        "# - Train with curriculum learning: start with reduced stress and small budgets, then increase difficulty.\n",
        "# - Use vectorized environments and GPU/CUDA for faster training; stable-baselines3 PPO benefits from\n",
        "#   larger batch sizes and a good policy/optimizer setup.\n",
        "\n",
        "# End of notebook with RL global resource manager"
      ],
