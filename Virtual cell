    {
      "cell_type": "code",
      "source": [
        "# Virtual Cell Simulation with Adaptive Safe RL & Hard Safety Shield\n",
        "# Colab-ready notebook\n",
        "\n",
        "# %%\n",
        "# 1) Install dependencies\n",
        "!pip install --quiet stable-baselines3 gym \"shimmy>=0.2.1\" matplotlib ipywidgets torch numpy\n",
        "\n",
        "# %%\n",
        "# 2) Imports\n",
        "import numpy as np\n",
        "import math, random, copy\n",
        "from collections import deque\n",
        "import matplotlib.pyplot as plt\n",
        "import gym\n",
        "from gym import spaces\n",
        "from stable_baselines3 import PPO\n",
        "from stable_baselines3.common.env_checker import check_env\n",
        "from IPython.display import clear_output\n",
        "# Import shimmy for Gym compatibility\n",
        "import shimmy\n",
        "\n",
        "# %%\n",
        "# 3) Core Biological Components\n",
        "\n",
        "class SyntheticTelomere:\n",
        "    def __init__(self, initial_length=1000.0, sequence_score=0.5, degradation_rate=1.0):\n",
        "        self.L = float(initial_length)\n",
        "        self.sequence_score = float(sequence_score)\n",
        "        self.degradation_rate = float(degradation_rate)\n",
        "\n",
        "    def shorten(self, divisions=1):\n",
        "        shortening = self.degradation_rate * (1 - 0.5 * self.sequence_score)\n",
        "        self.L = max(self.L - shortening * divisions, 0.0)\n",
        "        return self.L\n",
        "\n",
        "    def lengthen_by_telomerase(self, telomerase_activity):\n",
        "        extension = 5.0 * telomerase_activity * (0.5 + 0.5 * self.sequence_score)\n",
        "        self.L += extension\n",
        "        return self.L\n",
        "\n",
        "class PhagocytosisSystem:\n",
        "    def __init__(self, efficiency=0.5):\n",
        "        self.efficiency = efficiency\n",
        "\n",
        "    def cleanup(self, damage_load):\n",
        "        cleanup = self.efficiency * (damage_load ** 0.8)\n",
        "        new_damage = max(damage_load - cleanup, 0.0)\n",
        "        return new_damage, 0.01 * cleanup\n",
        "\n",
        "class VirtualCell:\n",
        "    def __init__(self, telomere):\n",
        "        self.telomere = telomere\n",
        "        self.alive = True\n",
        "        self.divisions = 0\n",
        "\n",
        "    def step_divide(self, telomerase_activity, damage, nutrient):\n",
        "        if not self.alive:\n",
        "            return False\n",
        "        p_div = 1.0 / (1.0 + math.exp(-(self.telomere.L - 200.0)/100.0))\n",
        "        p_div *= nutrient * max(0.0, 1.0 - min(1.0, damage/200.0))\n",
        "        if random.random() < p_div:\n",
        "            self.telomere.shorten(1)\n",
        "            self.telomere.lengthen_by_telomerase(telomerase_activity)\n",
        "            self.divisions += 1\n",
        "            if self.telomere.L <= 50.0 or damage > 150.0:\n",
        "                self.alive = False\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "# %%\n",
        "# 4) Tissue and Environment\n",
        "\n",
        "class Tissue:\n",
        "    def __init__(self, name, n_cells, nutrient_level, stress_factor, telomere_params=None):\n",
        "        self.name = name\n",
        "        self.nutrient_level = nutrient_level\n",
        "        self.stress_factor = stress_factor\n",
        "        self.cells = [VirtualCell(SyntheticTelomere(**(telomere_params or {}))) for _ in range(n_cells)]\n",
        "        self.damage = 0.0\n",
        "        self.phago = PhagocytosisSystem(0.6)\n",
        "        self.signaling_output = 0.0\n",
        "\n",
        "    def step(self, telomerase_activity, phago_boost):\n",
        "        base_damage = 0.5 * self.stress_factor + 0.05 * sum(1 for c in self.cells if c.alive and c.divisions > 0)\n",
        "        stress = np.random.gamma(shape=1.2, scale=self.stress_factor)\n",
        "        self.damage += base_damage + 0.2 * stress\n",
        "        self.phago.efficiency = np.clip(self.phago.efficiency + phago_boost, 0, 1)\n",
        "        self.damage, _ = self.phago.cleanup(self.damage)\n",
        "        divs = 0\n",
        "        for c in self.cells:\n",
        "            if c.alive:\n",
        "                if c.step_divide(telomerase_activity, self.damage, self.nutrient_level):\n",
        "                    divs += 1\n",
        "                    self.damage += 0.2\n",
        "        avg_tel = np.mean([c.telomere.L for c in self.cells if c.alive] or [0.0])\n",
        "        alive_frac = sum(1 for c in self.cells if c.alive)/len(self.cells)\n",
        "        self.signaling_output = np.tanh(self.damage/100.0) * 0.5 + np.tanh(1 - self.nutrient_level) * 0.5\n",
        "        return avg_tel, self.damage, alive_frac, divs, self.signaling_output\n",
        "\n",
        "class CrossTissueEnv:\n",
        "    def __init__(self, tissues_conf, diffusion_rate=0.05, signal_coupling=0.2):\n",
        "        self.tissues = [Tissue(**conf) for conf in tissues_conf]\n",
        "        self.diffusion_rate = diffusion_rate\n",
        "        self.signal_coupling = signal_coupling\n",
        "        self.time = 0\n",
        "\n",
        "    def diffuse_nutrients(self):\n",
        "        levels = np.array([t.nutrient_level for t in self.tissues])\n",
        "        mean_lvl = np.mean(levels)\n",
        "        for i, t in enumerate(self.tissues):\n",
        "            t.nutrient_level += self.diffusion_rate * (mean_lvl - t.nutrient_level)\n",
        "\n",
        "    def propagate_signals(self):\n",
        "        avg_signal = np.mean([t.signaling_output for t in self.tissues])\n",
        "        for t in self.tissues:\n",
        "            t.stress_factor = max(0.1, t.stress_factor * (1 - self.signal_coupling * avg_signal))\n",
        "\n",
        "    def step(self, actions):\n",
        "        results = []\n",
        "        for tissue, act in zip(self.tissues, actions):\n",
        "            results.append(tissue.step(*act))\n",
        "        self.diffuse_nutrients()\n",
        "        self.propagate_signals()\n",
        "        self.time += 1\n",
        "        return results\n",
        "\n",
        "# %%\n",
        "# 5) Adaptive Safe RL Environment\n",
        "\n",
        "class AdaptiveSafeGlobalResourceEnv(gym.Env):\n",
        "    def __init__(self, tissues_conf, total_nutrient_budget=3.0, lr_multiplier=0.01, ma_window=50):\n",
        "        super().__init__()\n",
        "        self.N = len(tissues_conf)\n",
        "        self.tissues_conf = tissues_conf # Store original config\n",
        "        self.env = CrossTissueEnv(tissues_conf)\n",
        "        self.total_budget = float(total_nutrient_budget)\n",
        "        self.max_steps = 400\n",
        "        self.current_step = 0\n",
        "        # Action/Obs spaces\n",
        "        self.action_space = spaces.Box(low=-2, high=2, shape=(3*self.N,), dtype=np.float32)\n",
        "        self.observation_space = spaces.Box(low=0, high=2000, shape=(5*self.N,), dtype=np.float32)\n",
        "        # Safety\n",
        "        self.nutrient_limit = 5.0\n",
        "        self.damage_limit = 200.0\n",
        "        self.telomere_limit = (100.0, 2000.0)\n",
        "        # Adaptive tracking\n",
        "        self.lambda_damage = 1.0\n",
        "        self.lr_multiplier = lr_multiplier\n",
        "        self.ma_window = ma_window\n",
        "        self.damage_history = deque(maxlen=ma_window)\n",
        "        self.nutr_history = deque(maxlen=ma_window)\n",
        "        self.dynamic_damage_limit = self.damage_limit\n",
        "        self.dynamic_nutrient_limit = self.nutrient_limit\n",
        "        self.dynamic_telomere_limit = list(self.telomere_limit)\n",
        "\n",
        "    def reset(self):\n",
        "        # Use stored config to recreate environment\n",
        "        self.env = CrossTissueEnv(self.tissues_conf)\n",
        "        self.current_step = 0\n",
        "        # Reset histories and dynamic limits for a clean start\n",
        "        self.damage_history.clear()\n",
        "        self.nutr_history.clear()\n",
        "        self.dynamic_damage_limit = self.damage_limit\n",
        "        self.dynamic_nutrient_limit = self.nutrient_limit\n",
        "        self.dynamic_telomere_limit = list(self.telomere_limit)\n",
        "        self.lambda_damage = 1.0\n",
        "        return self._obs()\n",
        "\n",
        "    def _obs(self):\n",
        "        obs = []\n",
        "        for t in self.env.tissues:\n",
        "            avg_tel = np.mean([c.telomere.L for c in t.cells if c.alive] or [0.0])\n",
        "            obs.extend([avg_tel, t.damage, sum(1 for c in t.cells if c.alive)/len(t.cells), t.nutrient_level, t.signaling_output])\n",
        "        return np.array(obs, dtype=np.float32)\n",
        "\n",
        "    def _apply_safety_constraints(self, allocations, telomerase, phago):\n",
        "        exp = np.exp(allocations - np.max(allocations))\n",
        "        fracs = exp / (np.sum(exp) + 1e-8)\n",
        "        safe_alloc = np.clip(fracs * self.total_budget, 0, self.dynamic_nutrient_limit)\n",
        "        telomerase = np.clip(telomerase, 0, 1)\n",
        "        phago = np.clip(phago, -0.2, 0.5)\n",
        "        return safe_alloc, telomerase, phago\n",
        "\n",
        "    def step(self, action):\n",
        "        action = np.array(action, dtype=np.float32)\n",
        "        alloc, tel, ph = np.split(action, 3)\n",
        "        safe_alloc, safe_tel, safe_ph = self._apply_safety_constraints(alloc, tel, ph)\n",
        "        actions = [(float(safe_tel[i]), float(safe_ph[i])) for i in range(self.N)]\n",
        "        for i, t in enumerate(self.env.tissues):\n",
        "            t.nutrient_level = float(safe_alloc[i])\n",
        "        results = self.env.step(actions)\n",
        "        # Update moving averages and dynamic limits\n",
        "        self.damage_history.append(np.mean([r[1] for r in results]))\n",
        "        self.nutr_history.append(np.mean([t.nutrient_level for t in self.env.tissues]))\n",
        "        if len(self.damage_history) >= 5:\n",
        "            dmg_mean = np.mean(self.damage_history)\n",
        "            self.dynamic_damage_limit = max(self.damage_limit*0.5, min(self.damage_limit*1.5, self.dynamic_damage_limit * (1.01 if dmg_mean < 0.6*self.damage_limit else 0.995)))\n",
        "        obs = self._obs()\n",
        "        reward = sum(np.tanh((r[0]-200)/800)+r[2]-0.01*r[1] for r in results)\n",
        "        # Safety penalty\n",
        "        for r in results:\n",
        "            if r[1] > self.dynamic_damage_limit:\n",
        "                reward -= self.lambda_damage * 2 * (r[1]/self.dynamic_damage_limit-1)\n",
        "        self.lambda_damage = max(0.1, self.lambda_damage + self.lr_multiplier * (sum(1 for r in results if r[1] > self.dynamic_damage_limit)-0.1))\n",
        "        self.current_step += 1\n",
        "        done = self.current_step >= self.max_steps or all(sum(1 for c in t.cells if c.alive)==0 for t in self.env.tissues)\n",
        "        return obs, float(reward), done, {'lambda_damage': self.lambda_damage, 'dynamic_damage_limit': self.dynamic_damage_limit}\n",
        "\n",
        "# %%\n",
        "# 6) Hard Safety Shield\n",
        "\n",
        "class SafetyShield:\n",
        "    def __init__(self, env, safe_budget_fraction=0.3):\n",
        "        self.env = env\n",
        "        self.interventions = []\n",
        "        self.safe_budget_fraction = safe_budget_fraction\n",
        "\n",
        "    def shield_action(self, action, step_idx=None):\n",
        "        # simple one-step clip: conservative action\n",
        "        N = len(self.env.env.tissues)\n",
        "        safe_act = np.concatenate([np.full(N, self.env.total_budget*self.safe_budget_fraction/N),\n",
        "                                   np.full(N, 0.2),\n",
        "                                   np.full(N, 0.2)])\n",
        "        # Check if intervention needed (damage limit exceeded, simple proxy)\n",
        "        obs = self.env._obs()\n",
        "        predicted_damage = np.mean(obs[1::5])\n",
        "        if predicted_damage > self.env.dynamic_damage_limit:\n",
        "            self.interventions.append({'step': step_idx, 'original_action': action, 'safe_action': safe_act})\n",
        "            return safe_act, True\n",
        "        return action, False\n",
        "\n",
        "class ShieldedEnvWrapper(gym.Env):\n",
        "    def __init__(self, base_env, shield: SafetyShield):\n",
        "        super().__init__()\n",
        "        self.base_env = base_env\n",
        "        self.shield = shield\n",
        "        self.action_space = base_env.action_space\n",
        "        self.observation_space = base_env.observation_space\n",
        "\n",
        "    def reset(self):\n",
        "        return self.base_env.reset()\n",
        "\n",
        "    def step(self, action):\n",
        "        action, intervened = self.shield.shield_action(action, step_idx=getattr(self.base_env, 'current_step', None))\n",
        "        obs, rew, done, info = self.base_env.step(action)\n",
        "        info['shield_intervened'] = intervened\n",
        "        if intervened: info['shield_log'] = self.shield.interventions[-1]\n",
        "        return obs, rew, done, info\n",
        "\n",
        "    def render(self, mode='human'):\n",
        "        return self.base_env.render(mode)\n",
        "\n",
        "# %%\n",
        "# 7) Demo: Live Plot Training\n",
        "\n",
        "tissues_conf_demo = [\n",
        "    {'name': 'Liver', 'n_cells': 10, 'nutrient_level': 1.0, 'stress_factor': 1.0, 'telomere_params': {'sequence_score': 0.7}},\n",
        "    {'name': 'Heart', 'n_cells': 8, 'nutrient_level': 0.8, 'stress_factor': 1.2, 'telomere_params': {'sequence_score': 0.6}},\n",
        "    {'name': 'Brain', 'n_cells': 6, 'nutrient_level': 0.9, 'stress_factor': 0.7, 'telomere_params': {'sequence_score': 0.8}}\n",
        "]\n",
        "\n",
        "base_env_demo = AdaptiveSafeGlobalResourceEnv(tissues_conf_demo, total_nutrient_budget=3.0)\n",
        "shield_demo = SafetyShield(base_env_demo, safe_budget_fraction=0.3)\n",
        "env_demo = ShieldedEnvWrapper(base_env_demo, shield_demo)\n",
        "\n",
        "model = PPO('MlpPolicy', env_demo, verbose=0)\n",
        "\n",
        "# Tracking metrics\n",
        "steps = []\n",
        "lambda_history = []\n",
        "damage_limit_history = []\n",
        "shield_count_history = []\n",
        "\n",
        "obs = env_demo.reset()\n",
        "done = False\n",
        "step = 0\n",
        "shield_interventions = 0\n",
        "\n",
        "while not done and step < 100:\n",
        "    action, _ = model.predict(obs, deterministic=True)\n",
        "    obs, reward, done, info = env_demo.step(action)\n",
        "    steps.append(step)\n",
        "    lambda_history.append(getattr(base_env_demo, 'lambda_damage', 1.0))\n",
        "    damage_limit_history.append(getattr(base_env_demo, 'dynamic_damage_limit', 200.0))\n",
        "    if info.get('shield_intervened', False): shield_interventions += 1\n",
        "    shield_count_history.append(shield_interventions)\n",
        "    # live plot\n",
        "    clear_output(wait=True)\n",
        "    plt.figure(figsize=(10,4))\n",
        "    plt.plot(steps, lambda_history, label='Lambda Damage')\n",
        "    plt.plot(steps, damage_limit_history, label='Dynamic Damage Limit')\n",
        "    plt.plot(steps, shield_count_history, label='Cumulative Shield Interventions')\n",
        "    plt.xlabel('Step')\n",
        "    plt.ylabel('Value')\n",
        "    plt.title('Adaptive RL Metrics with Hard Safety Shield')\n",
        "    plt.legend()\n",
        "    plt.grid(True)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "    step += 1\n",
        "\n",
        "print(\"Live plot demo complete.\")\n",
        "print(f\"Total shield interventions: {shield_interventions}\")"
      ],
