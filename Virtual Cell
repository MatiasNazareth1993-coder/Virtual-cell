      ],
      "source": [
        "import numpy as np\n",
        "import random\n",
        "\n",
        "class AdaptiveCell:\n",
        "    def __init__(self, initial_telomere=1000, critical_telomere=200):\n",
        "        self.telomere = initial_telomere\n",
        "        self.critical_telomere = critical_telomere\n",
        "        self.nutrients = 100\n",
        "        self.waste = 0\n",
        "        self.damaged_organelles = 0\n",
        "        self.age = 0\n",
        "        self.divisions = 0\n",
        "        self.telomerase_active = False\n",
        "        self.phagocytosis_level = 1  # 0: off, 1: normal, 2: high\n",
        "\n",
        "    def step(self, action):\n",
        "        # Actions: [telomerase_on/off, phagocytosis_level 0/1/2]\n",
        "        self.telomerase_active = bool(action[0])\n",
        "        self.phagocytosis_level = action[1]\n",
        "\n",
        "        # Phagocytosis clears waste & organelles\n",
        "        cleared_waste = min(self.waste, np.random.randint(5, 15) * self.phagocytosis_level)\n",
        "        cleared_organelles = min(self.damaged_organelles, np.random.randint(1, 5) * self.phagocytosis_level)\n",
        "        self.waste -= cleared_waste\n",
        "        self.damaged_organelles -= cleared_organelles\n",
        "        self.nutrients += cleared_waste + cleared_organelles\n",
        "\n",
        "        # Telomerase repair\n",
        "        if self.telomerase_active and self.telomere <= self.critical_telomere:\n",
        "            repair = np.random.randint(50, 150)\n",
        "            self.telomere += repair\n",
        "\n",
        "        # Cell division\n",
        "        can_divide = self.telomere > self.critical_telomere and self.nutrients > 50 and self.waste < 30\n",
        "        if can_divide:\n",
        "            self.telomere -= np.random.randint(20, 50)\n",
        "            self.nutrients -= 30\n",
        "            self.divisions += 1\n",
        "\n",
        "        # Age the cell\n",
        "        self.age += 1\n",
        "        self.waste += np.random.randint(5, 15)\n",
        "        self.damaged_organelles += np.random.randint(1, 5)\n",
        "        self.nutrients -= np.random.randint(5, 10)\n",
        "\n",
        "        # Reward function\n",
        "        reward = 0\n",
        "        if can_divide:\n",
        "            reward += 10\n",
        "        reward -= self.waste * 0.1\n",
        "        if self.telomere < self.critical_telomere:\n",
        "            reward -= 5\n",
        "        if self.nutrients <= 0:\n",
        "            reward -= 20\n",
        "        return self.get_state(), reward\n",
        "\n",
        "    def get_state(self):\n",
        "        # Discretize state for Q-learning\n",
        "        return (self.telomere // 50, self.nutrients // 10, self.waste // 10, self.damaged_organelles // 5)\n",
        "\n",
        "# Q-Learning agent\n",
        "class QAgent:\n",
        "    def __init__(self, actions):\n",
        "        self.q_table = {}\n",
        "        self.actions = actions\n",
        "        self.lr = 0.1\n",
        "        self.gamma = 0.9\n",
        "        self.epsilon = 0.2\n",
        "\n",
        "    def choose_action(self, state):\n",
        "        if random.random() < self.epsilon:\n",
        "            return random.choice(self.actions)\n",
        "        if state not in self.q_table:\n",
        "            self.q_table[state] = {a: 0 for a in self.actions}\n",
        "        return max(self.q_table[state], key=self.q_table[state].get)\n",
        "\n",
        "    def learn(self, state, action, reward, next_state):\n",
        "        if state not in self.q_table:\n",
        "            self.q_table[state] = {a: 0 for a in self.actions}\n",
        "        if next_state not in self.q_table:\n",
        "            self.q_table[next_state] = {a: 0 for a in self.actions}\n",
        "        predict = self.q_table[state][action]\n",
        "        target = reward + self.gamma * max(self.q_table[next_state].values())\n",
        "        self.q_table[state][action] += self.lr * (target - predict)\n",
        "\n",
        "# Define possible actions\n",
        "actions = [(0,0), (0,1), (0,2), (1,0), (1,1), (1,2)]\n",
        "\n",
        "# Training loop\n",
        "episodes = 500\n",
        "agent = QAgent(actions)\n",
        "\n",
        "for ep in range(episodes):\n",
        "    cell = AdaptiveCell()\n",
        "    state = cell.get_state()\n",
        "    for t in range(50):\n",
        "        action = agent.choose_action(state)\n",
        "        next_state, reward = cell.step(action)\n",
        "        agent.learn(state, action, reward, next_state)\n",
        "        state = next_state\n",
        "\n",
        "print(\"Training complete. Learned Q-values:\")\n",
        "for state, qvals in list(agent.q_table.items())[:5]:  # show first 5 states\n",
        "    print(state, qvals)\n"
      ]
    },
    {
