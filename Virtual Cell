      "source": [
        "import numpy as np\n",
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.optim as optim\n",
        "import random\n",
        "\n",
        "# Define Deep Q-Network\n",
        "class DQN(nn.Module):\n",
        "    def __init__(self, input_dim, output_dim):\n",
        "        super(DQN, self).__init__()\n",
        "        self.fc1 = nn.Linear(input_dim, 64)\n",
        "        self.fc2 = nn.Linear(64, 64)\n",
        "        self.fc3 = nn.Linear(64, output_dim)\n",
        "\n",
        "    def forward(self, x):\n",
        "        x = torch.relu(self.fc1(x))\n",
        "        x = torch.relu(self.fc2(x))\n",
        "        return self.fc3(x)\n",
        "\n",
        "# Adaptive cell with DRL agent\n",
        "class DRLCell:\n",
        "    def __init__(self, env_state_dim=7, action_size=6, gamma=0.9, lr=0.001):\n",
        "        self.telomere = 1000\n",
        "        self.critical_telomere = 200\n",
        "        self.nutrients = 100\n",
        "        self.waste = 0\n",
        "        self.damaged_organelles = 0\n",
        "        self.age = 0\n",
        "        self.divisions = 0\n",
        "\n",
        "        self.env_state_dim = env_state_dim\n",
        "        self.action_size = action_size\n",
        "        self.gamma = gamma\n",
        "\n",
        "        self.model = DQN(env_state_dim, action_size)\n",
        "        self.optimizer = optim.Adam(self.model.parameters(), lr=lr)\n",
        "        self.criterion = nn.MSELoss()\n",
        "\n",
        "        self.memory = []  # experience replay\n",
        "        self.epsilon = 1.0\n",
        "        self.epsilon_decay = 0.995\n",
        "        self.epsilon_min = 0.05\n",
        "\n",
        "    def get_state(self, env_nutrients, env_waste):\n",
        "        return np.array([\n",
        "            self.telomere / 1000,\n",
        "            self.nutrients / 100,\n",
        "            self.waste / 50,\n",
        "            self.damaged_organelles / 50,\n",
        "            self.age / 50,\n",
        "            env_nutrients / 500,\n",
        "            env_waste / 500\n",
        "        ], dtype=np.float32)\n",
        "\n",
        "    def choose_action(self, state):\n",
        "        if random.random() < self.epsilon:\n",
        "            return random.randint(0, self.action_size - 1)\n",
        "        state_tensor = torch.tensor(state).float().unsqueeze(0)\n",
        "        with torch.no_grad():\n",
        "            q_values = self.model(state_tensor)\n",
        "        return int(torch.argmax(q_values))\n",
        "\n",
        "    def remember(self, state, action, reward, next_state, done):\n",
        "        self.memory.append((state, action, reward, next_state, done))\n",
        "        if len(self.memory) > 5000:\n",
        "            self.memory.pop(0)\n",
        "\n",
        "    def replay(self, batch_size=32):\n",
        "        if len(self.memory) < batch_size:\n",
        "            return\n",
        "        batch = random.sample(self.memory, batch_size)\n",
        "        for state, action, reward, next_state, done in batch:\n",
        "            state_tensor = torch.tensor(state).float()\n",
        "            next_state_tensor = torch.tensor(next_state).float()\n",
        "            target = reward\n",
        "            if not done:\n",
        "                target += self.gamma * torch.max(self.model(next_state_tensor)).item()\n",
        "            output = self.model(state_tensor)[action]\n",
        "            loss = self.criterion(output, torch.tensor(target))\n",
        "            self.optimizer.zero_grad()\n",
        "            loss.backward()\n",
        "            self.optimizer.step()\n",
        "        # Decay epsilon\n",
        "        if self.epsilon > self.epsilon_min:\n",
        "            self.epsilon *= self.epsilon_decay\n",
        "\n",
        "    def step(self, action_idx):\n",
        "        # Decode action index\n",
        "        telomerase_on = action_idx // 3\n",
        "        phago_level = action_idx % 3\n",
        "\n",
        "        # Phagocytosis\n",
        "        cleared_waste = min(self.waste, np.random.randint(5, 15) * phago_level)\n",
        "        cleared_organelles = min(self.damaged_organelles, np.random.randint(1, 5) * phago_level)\n",
        "        self.waste -= cleared_waste\n",
        "        self.damaged_organelles -= cleared_organelles\n",
        "        self.nutrients += cleared_waste + cleared_organelles\n",
        "\n",
        "        # Telomerase repair\n",
        "        if telomerase_on and self.telomere <= self.critical_telomere:\n",
        "            self.telomere += np.random.randint(50, 150)\n",
        "\n",
        "        # Cell division\n",
        "        can_divide = self.telomere > self.critical_telomere and self.nutrients > 50 and self.waste < 30\n",
        "        if can_divide:\n",
        "            self.telomere -= np.random.randint(20, 50)\n",
        "            self.nutrients -= 30\n",
        "            self.divisions += 1\n",
        "\n",
        "        # Age cell\n",
        "        self.age += 1\n",
        "        self.waste += np.random.randint(5, 15)\n",
        "        self.damaged_organelles += np.random.randint(1, 5)\n",
        "        self.nutrients -= np.random.randint(5, 10)\n",
        "\n",
        "        # Reward\n",
        "        reward = 0\n",
        "        if can_divide:\n",
        "            reward += 10\n",
        "        reward -= self.waste * 0.1\n",
        "        if self.telomere < self.critical_telomere:\n",
        "            reward -= 5\n",
        "        if self.nutrients <= 0:\n",
        "            reward -= 20\n",
        "        done = self.nutrients <= 0 or self.age >= 50\n",
        "        return reward, done\n",
        "\n",
        "# Multi-cell DRL environment\n",
        "class DRLEnvironment:\n",
        "    def __init__(self, num_cells=5):\n",
        "        self.num_cells = num_cells\n",
        "        self.env_nutrients = 500\n",
        "        self.env_waste = 0\n",
        "        self.cells = [DRLCell() for _ in range(num_cells)]\n",
        "\n",
        "    def step(self):\n",
        "        rewards = []\n",
        "        done_flags = []\n",
        "        for cell in self.cells:\n",
        "            state = cell.get_state(self.env_nutrients, self.env_waste)\n",
        "            action = cell.choose_action(state)\n",
        "            reward, done = cell.step(action)\n",
        "            next_state = cell.get_state(self.env_nutrients, self.env_waste)\n",
        "            cell.remember(state, action, reward, next_state, done)\n",
        "            rewards.append(reward)\n",
        "            done_flags.append(done)\n",
        "            self.env_waste += cell.waste * 0.2\n",
        "            self.env_nutrients -= min(cell.nutrients, self.env_nutrients // self.num_cells)\n",
        "        for cell in self.cells:\n",
        "            cell.replay()\n",
        "        return rewards, done_flags\n",
        "\n",
        "# Example training loop\n",
        "env = DRLEnvironment(num_cells=5)\n",
        "episodes = 50\n",
        "\n",
        "for ep in range(episodes):\n",
        "    done_all = [False]*env.num_cells\n",
        "    step_count = 0\n",
        "    while not all(done_all):\n",
        "        rewards, done_flags = env.step()\n",
        "        done_all = [done_all[i] or done_flags[i] for i in range(env.num_cells)]\n",
        "        step_count += 1\n",
        "    print(f\"Episode {ep+1} finished in {step_count} steps\")\n"
      ]
    },
