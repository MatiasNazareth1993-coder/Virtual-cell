        "!pip install gymnasium stable-baselines3\n",
        "\n",
        "import gymnasium as gym\n",
        "from gymnasium import spaces\n",
        "import numpy as np\n",
        "\n",
        "class RLOrganEnv(gym.Env):\n",
        "    def __init__(self, num_tissues=2, cells_per_tissue=5):\n",
        "        super(RLOrganEnv, self).__init__()\n",
        "        self.env = MultiTissueEnv(num_tissues=num_tissues, cells_per_tissue=cells_per_tissue)\n",
        "        self.num_tissues = num_tissues\n",
        "\n",
        "        # Observation: avg telomere, avg nutrients, env waste per tissue\n",
        "        self.observation_space = spaces.Box(low=0, high=10000, shape=(num_tissues*3,), dtype=np.float32)\n",
        "\n",
        "        # Actions: nutrient injection, telomerase multiplier, phagocytosis multiplier per tissue\n",
        "        self.action_space = spaces.Box(low=np.array([0,0.5,0.5]*num_tissues),\n",
        "                                       high=np.array([50,2.0,2.0]*num_tissues),\n",
        "                                       dtype=np.float32)\n",
        "\n",
        "    def reset(self, seed=None, options=None):\n",
        "        super().reset(seed=seed)\n",
        "        self.env = MultiTissueEnv(num_tissues=self.num_tissues, cells_per_tissue=len(self.env.tissues[0].cells))\n",
        "        return self._get_obs(), {}\n",
        "\n",
        "    def step(self, actions):\n",
        "        # Apply actions\n",
        "        for i, tissue in enumerate(self.env.tissues):\n",
        "            tissue.env_nutrients += actions[i*3]\n",
        "            tissue.drug_telomerase = actions[i*3 + 1]\n",
        "            tissue.drug_phago = actions[i*3 + 2]\n",
        "\n",
        "        self.env.step()\n",
        "        obs = self._get_obs()\n",
        "        reward = self._compute_reward()\n",
        "        done = False  # could define termination conditions\n",
        "        return obs, reward, done, False, {}\n",
        "\n",
        "    def _get_obs(self):\n",
        "        obs = []\n",
        "        for tissue in self.env.tissues:\n",
        "            avg_tel = np.mean([cell.telomere for cell in tissue.cells])\n",
        "            avg_nut = np.mean([cell.nutrients for cell in tissue.cells])\n",
        "            waste = tissue.env_waste\n",
        "            obs.extend([avg_tel, avg_nut, waste])\n",
        "        return np.array(obs, dtype=np.float32)\n",
        "\n",
        "    def _compute_reward(self):\n",
        "        reward = 0\n",
        "        for tissue in self.env.tissues:\n",
        "            avg_tel = np.mean([cell.telomere for cell in tissue.cells])\n",
        "            avg_nut = np.mean([cell.nutrients for cell in tissue.cells])\n",
        "            waste = tissue.env_waste\n",
        "            reward += avg_tel*0.5 + avg_nut*0.3 - waste*0.2\n",
        "        return reward"
      ]
