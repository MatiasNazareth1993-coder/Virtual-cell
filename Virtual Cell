      "source": [
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "# Initialize metrics dataframe\n",
        "def init_metrics(cells, episodes):\n",
        "    metrics = {\n",
        "        \"Episode\": [],\n",
        "        \"Step\": [],\n",
        "        \"Env_Nutrients\": [],\n",
        "        \"Env_Waste\": []\n",
        "    }\n",
        "    for i in range(len(cells)):\n",
        "        metrics[f\"Cell{i}_Telomere\"] = []\n",
        "        metrics[f\"Cell{i}_Nutrients\"] = []\n",
        "        metrics[f\"Cell{i}_Waste\"] = []\n",
        "        metrics[f\"Cell{i}_Divisions\"] = []\n",
        "    return pd.DataFrame(metrics)\n",
        "\n",
        "# Collect metrics at each step\n",
        "def collect_metrics(df, ep, step, env):\n",
        "    row = {\n",
        "        \"Episode\": ep,\n",
        "        \"Step\": step,\n",
        "        \"Env_Nutrients\": env.env_nutrients,\n",
        "        \"Env_Waste\": env.env_waste\n",
        "    }\n",
        "    for i, cell in enumerate(env.cells):\n",
        "        row[f\"Cell{i}_Telomere\"] = cell.telomere\n",
        "        row[f\"Cell{i}_Nutrients\"] = cell.nutrients\n",
        "        row[f\"Cell{i}_Waste\"] = cell.waste\n",
        "        row[f\"Cell{i}_Divisions\"] = cell.divisions\n",
        "    df = pd.concat([df, pd.DataFrame([row])], ignore_index=True)\n",
        "    return df\n",
        "\n",
        "# Example training loop with metrics\n",
        "env = DRLEnvironment(num_cells=5)\n",
        "episodes = 10  # fewer episodes for demonstration\n",
        "metrics_df = init_metrics(env.cells, episodes)\n",
        "\n",
        "for ep in range(episodes):\n",
        "    done_all = [False]*env.num_cells\n",
        "    step_count = 0\n",
        "    while not all(done_all):\n",
        "        rewards, done_flags = env.step()\n",
        "        done_all = [done_all[i] or done_flags[i] for i in range(env.num_cells)]\n",
        "        metrics_df = collect_metrics(metrics_df, ep, step_count, env)\n",
        "        step_count += 1\n",
        "    print(f\"Episode {ep+1} finished in {step_count} steps\")\n",
        "\n",
        "# --- Visualization ---\n",
        "\n",
        "# Plot telomere lengths for each cell\n",
        "plt.figure(figsize=(12,6))\n",
        "for i in range(env.num_cells):\n",
        "    plt.plot(metrics_df[f\"Cell{i}_Telomere\"], label=f\"Cell {i} Telomere\")\n",
        "plt.xlabel(\"Step\")\n",
        "plt.ylabel(\"Telomere Length\")\n",
        "plt.title(\"Telomere Dynamics per Cell\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# Plot environment nutrients and waste\n",
        "plt.figure(figsize=(12,6))\n",
        "plt.plot(metrics_df[\"Env_Nutrients\"], label=\"Environment Nutrients\")\n",
        "plt.plot(metrics_df[\"Env_Waste\"], label=\"Environment Waste\")\n",
        "plt.xlabel(\"Step\")\n",
        "plt.ylabel(\"Amount\")\n",
        "plt.title(\"Environment Nutrients & Waste Dynamics\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# Plot cell divisions\n",
        "plt.figure(figsize=(12,6))\n",
        "for i in range(env.num_cells):\n",
        "    plt.plot(metrics_df[f\"Cell{i}_Divisions\"], label=f\"Cell {i} Divisions\")\n",
        "plt.xlabel(\"Step\")\n",
        "plt.ylabel(\"Divisions\")\n",
        "plt.title(\"Cell Divisions Over Time\")\n",
        "plt.legend()\n",
        "plt.show()\n"
      ]
