      "execution_count": 3,
      "metadata": {
        "id": "dMklal62SyOV"
      },
      "outputs": [],
      "source": [
        "# --- Multi-Tissue Environment --- #\n",
        "class Tissue:\n",
        "    def __init__(self, num_cells=5, name=\"Tissue\"):\n",
        "        self.name = name\n",
        "        self.cells = [DRLCell() for _ in range(num_cells)]\n",
        "        self.env_nutrients = 500\n",
        "        self.env_waste = 0\n",
        "        self.drug_telomerase = 1.0\n",
        "        self.drug_phago = 1.0\n",
        "\n",
        "    def step(self):\n",
        "        rewards = []\n",
        "        for cell in self.cells:\n",
        "            # Apply drug multipliers\n",
        "            cell.phagocytosis_level = int(cell.phagocytosis_level * self.drug_phago)\n",
        "            cell.telomerase_active = cell.telomerase_active  # can also multiply efficiency if desired\n",
        "\n",
        "            reward, done = cell.step(cell.telomerase_active)\n",
        "            # Update tissue environment\n",
        "            self.env_waste += cell.waste * 0.2\n",
        "            self.env_nutrients -= min(cell.nutrients, self.env_nutrients // len(self.cells))\n",
        "            rewards.append(reward)\n",
        "        return rewards\n",
        "\n",
        "class MultiTissueEnv:\n",
        "    def __init__(self, num_tissues=2, cells_per_tissue=5):\n",
        "        self.tissues = [Tissue(num_cells=cells_per_tissue, name=f\"Tissue {i}\") for i in range(num_tissues)]\n",
        "\n",
        "    def step(self):\n",
        "        for tissue in self.tissues:\n",
        "            tissue.step()\n"
      ]
    },
