      "source": [
        "import numpy as np\n",
        "import random\n",
        "\n",
        "# Multi-cell environment\n",
        "class MultiCellEnvironment:\n",
        "    def __init__(self, num_cells=5, initial_nutrients=500):\n",
        "        self.num_cells = num_cells\n",
        "        self.nutrients = initial_nutrients\n",
        "        self.env_waste = 0\n",
        "        self.cells = [AdaptiveCell() for _ in range(num_cells)]\n",
        "\n",
        "    def step(self, actions):\n",
        "        # actions: list of tuples [(telomerase, phagocytosis), ...] for each cell\n",
        "        rewards = []\n",
        "        for i, cell in enumerate(self.cells):\n",
        "            cell_action = actions[i]\n",
        "\n",
        "            # Consume nutrients from shared pool\n",
        "            nutrient_consumed = min(cell.nutrients, self.nutrients // self.num_cells)\n",
        "            cell.nutrients += nutrient_consumed\n",
        "            self.nutrients -= nutrient_consumed\n",
        "\n",
        "            # Step the cell\n",
        "            state, reward = cell.step(cell_action)\n",
        "\n",
        "            # Waste released to environment\n",
        "            self.env_waste += cell.waste * 0.2  # portion of waste enters environment\n",
        "\n",
        "            rewards.append(reward)\n",
        "        return rewards\n",
        "\n",
        "# Cell class with adaptive features (as before)\n",
        "class AdaptiveCell:\n",
        "    def __init__(self, initial_telomere=1000, critical_telomere=200):\n",
        "        self.telomere = initial_telomere\n",
        "        self.critical_telomere = critical_telomere\n",
        "        self.nutrients = 100\n",
        "        self.waste = 0\n",
        "        self.damaged_organelles = 0\n",
        "        self.age = 0\n",
        "        self.divisions = 0\n",
        "        self.telomerase_active = False\n",
        "        self.phagocytosis_level = 1\n",
        "\n",
        "    def step(self, action):\n",
        "        self.telomerase_active = bool(action[0])\n",
        "        self.phagocytosis_level = action[1]\n",
        "\n",
        "        # Phagocytosis: remove waste and organelles\n",
        "        cleared_waste = min(self.waste, np.random.randint(5, 15) * self.phagocytosis_level)\n",
        "        cleared_organelles = min(self.damaged_organelles, np.random.randint(1, 5) * self.phagocytosis_level)\n",
        "        self.waste -= cleared_waste\n",
        "        self.damaged_organelles -= cleared_organelles\n",
        "        self.nutrients += cleared_waste + cleared_organelles\n",
        "\n",
        "        # Telomerase repair\n",
        "        if self.telomerase_active and self.telomere <= self.critical_telomere:\n",
        "            repair = np.random.randint(50, 150)\n",
        "            self.telomere += repair\n",
        "\n",
        "        # Cell division\n",
        "        can_divide = self.telomere > self.critical_telomere and self.nutrients > 50 and self.waste < 30\n",
        "        if can_divide:\n",
        "            self.telomere -= np.random.randint(20, 50)\n",
        "            self.nutrients -= 30\n",
        "            self.divisions += 1\n",
        "\n",
        "        # Age cell\n",
        "        self.age += 1\n",
        "        self.waste += np.random.randint(5, 15)\n",
        "        self.damaged_organelles += np.random.randint(1, 5)\n",
        "        self.nutrients -= np.random.randint(5, 10)\n",
        "\n",
        "        # Reward function\n",
        "        reward = 0\n",
        "        if can_divide:\n",
        "            reward += 10\n",
        "        reward -= self.waste * 0.1\n",
        "        if self.telomere < self.critical_telomere:\n",
        "            reward -= 5\n",
        "        if self.nutrients <= 0:\n",
        "            reward -= 20\n",
        "        return self.get_state(), reward\n",
        "\n",
        "    def get_state(self):\n",
        "        return (self.telomere // 50, self.nutrients // 10, self.waste // 10, self.damaged_organelles // 5)\n",
        "\n",
        "# Example simulation\n",
        "env = MultiCellEnvironment(num_cells=5, initial_nutrients=500)\n",
        "actions_list = [(1,1), (0,2), (1,0), (0,1), (1,2)]  # example actions for 5 cells\n",
        "\n",
        "for t in range(20):\n",
        "    rewards = env.step(actions_list)\n",
        "    print(f\"Step {t+1}, Environment nutrients: {env.nutrients}, Waste: {env.env_waste}\")\n",
        "    print(f\"Cell rewards: {rewards}\")\n"
      ]
    },
