      "source": [
        "import numpy as np\n",
        "\n",
        "class VirtualCell:\n",
        "    def __init__(self, initial_telomere=1000, critical_telomere=200):\n",
        "        self.telomere_length = initial_telomere\n",
        "        self.critical_telomere = critical_telomere\n",
        "        self.telomerase_active = False\n",
        "        self.nutrients = 100\n",
        "        self.waste = 0\n",
        "        self.damaged_organelles = 0\n",
        "        self.age = 0\n",
        "        self.divisions = 0\n",
        "\n",
        "    def update_telomerase(self):\n",
        "        # Activate telomerase only when telomeres are critically short and nutrients are sufficient\n",
        "        if self.telomere_length <= self.critical_telomere and self.nutrients > 20:\n",
        "            self.telomerase_active = True\n",
        "        else:\n",
        "            self.telomerase_active = False\n",
        "\n",
        "    def repair_telomeres(self):\n",
        "        if self.telomerase_active:\n",
        "            repair_amount = np.random.randint(50, 150)  # Random repair within a range\n",
        "            self.telomere_length += repair_amount\n",
        "            print(f\"Telomerase activated: repaired {repair_amount} telomere units.\")\n",
        "\n",
        "    def perform_phagocytosis(self):\n",
        "        # Clear damaged organelles and waste\n",
        "        cleared_waste = min(self.waste, np.random.randint(5, 15))\n",
        "        cleared_organelles = min(self.damaged_organelles, np.random.randint(1, 5))\n",
        "        self.waste -= cleared_waste\n",
        "        self.damaged_organelles -= cleared_organelles\n",
        "        self.nutrients += cleared_waste + cleared_organelles\n",
        "        print(f\"Phagocytosis: cleared {cleared_waste} waste and {cleared_organelles} organelles.\")\n",
        "\n",
        "    def cell_division(self):\n",
        "        if self.telomere_length > self.critical_telomere and self.nutrients > 50 and self.waste < 30:\n",
        "            self.telomere_length -= np.random.randint(20, 50)  # Telomere shortening per division\n",
        "            self.nutrients -= 30\n",
        "            self.divisions += 1\n",
        "            print(f\"Cell divided. Total divisions: {self.divisions}.\")\n",
        "        else:\n",
        "            print(\"Cell cannot divide: conditions not met.\")\n",
        "\n",
        "    def age_cell(self):\n",
        "        self.age += 1\n",
        "        self.waste += np.random.randint(5, 15)\n",
        "        self.damaged_organelles += np.random.randint(1, 5)\n",
        "        self.nutrients -= np.random.randint(5, 10)\n",
        "        self.update_telomerase()\n",
        "        self.repair_telomeres()\n",
        "        self.perform_phagocytosis()\n",
        "        self.cell_division()\n",
        "        print(f\"Age: {self.age}, Telomere: {self.telomere_length}, Nutrients: {self.nutrients}, Waste: {self.waste}\")\n",
        "\n",
        "# Simulate for 50 time steps\n",
        "cell = VirtualCell()\n",
        "for _ in range(50):\n",
        "    cell.age_cell()"
      ]
    },
